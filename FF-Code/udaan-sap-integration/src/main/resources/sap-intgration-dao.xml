	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="sapIntegrationDAO" class="com.ff.sap.integration.dao.SAPIntegrationDAOImpl">
		<property name="emailSenderUtil" ref="emailSenderUtil" />
		<property name="sessionFactory" ref="hibernateSessionFactory" />
		
	</bean>
	
	<bean id="sapOrganizationCommonDAO" class="com.ff.universe.organization.dao.OrganizationCommonDAOImpl">
