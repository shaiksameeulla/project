	</bean>
	
	<bean id="sapGeographyServiceDAO" class="com.ff.universe.geography.dao.GeographyServiceDAOImpl">
		<property name="sessionFactory" ref="hibernateSessionFactory" />
	</bean>
	
	<bean id="pickUpCalculationCommissionDAO" class="com.ff.sap.integration.pickup.dao.PickUpCalculationCommissionDaoImpl">
	    <property name="emailSenderUtil" ref="emailSenderUtil" />
		<property name="sessionFactory" ref="hibernateSessionFactory" />
	</bean>
	
	<!-- Consignment Rate Calculation START -->
	<bean id="consignmentRateCalculationDAO" class="com.ff.sap.integration.consignmentratecalculation.dao.ConsignmentRateCalculationDAOImpl">
