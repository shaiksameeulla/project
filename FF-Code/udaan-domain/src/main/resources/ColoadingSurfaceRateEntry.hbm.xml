<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ff.domain.coloading.ColoadingSurfaceRateEntryDO" table="ff_d_coloading_surface_rate_entry">
        <id name="id" type="java.lang.Integer">
            <column name="ID" />
            <generator class="native" />
        </id>
        <property name="vendorId" type="integer">
            <column name="VENDOR_ID" length="11" not-null="true" />
        </property>
        <property name="fromDate" type="date">
            <column name="FROM_DATE" length="10" not-null="true" />
        </property>
        <property name="toWeight" type="integer">
            <column name="TO_WEIGHT"  length="11" not-null="true" />
        </property>
        <property name="additionalPerKg" type="double">
            <column name="ADDITIONAL_PER_KG" length="50" not-null="true" />
        </property>
        <property name="rate" type="double">
            <column name="RATE" precision="10" not-null="true" />
        </property>
        <property name="createdBy" type="int">
            <column name="CREATED_BY" not-null="true" />
        </property>
        <property name="updatedBy" type="int">
            <column name="UPDATED_BY" not-null="true" />
        </property>
        <property name="createdDate" type="timestamp">
            <column name="CREATED_DATE" length="19" not-null="true" />
        </property>
        <property name="updatedDate" type="timestamp">
            <column name="UPDATED_DATE" length="19" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
