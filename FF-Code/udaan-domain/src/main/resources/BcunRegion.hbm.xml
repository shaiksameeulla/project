<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.geography.BcunRegionDO" table="ff_d_region">
		<id name="regionId" column="REGION_ID" type="java.lang.Integer" >
			<generator class="assigned" />
		</id>
		
		<property name="regionCode"  column="REGION_CODE" />
		<property name="regionName" column="REGION_NAME"/>
		<property  name="regionDisplayName" column="REGION_DISPLAY_NAME"/>
		<property  name="zone" column="ZONE" />	
		<property name="dtToBranch"><column name="DT_TO_BRANCH" />
		</property>		
	</class>


</hibernate-mapping>