<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ff.domain.delivery.DeliveryNavigatorDO" table="ff_f_delivery_navigator">
		<id name="deliveryNavigatorId" type="java.lang.Long">
			<column name="DELIVERY_NAVIGATOR_ID" />
			<generator class="native" />
		</id>
		<property name="drsNumber" column="DRS_NUMBER" update="false"/>
		<property name="createdScreenCode" column="CREATED_BY_SCREEN_CODE" update="false" />
		<property name="fromScreenCode" column="FROM_SCREEN_CODE" update="false"/>
		<property name="navigateToUrl" column="NAVIGATE_TO_URL" update="false" />
		<property name="transactionCreateDate" column="TRANS_CREATED_DATE_TIME" />
		 <property name="dtToCentral">
			<column name="DT_TO_CENTRAL" />
		</property>  
		<!-- Start of Changes by <48206> for opsaman on 20/09/2013  -->
		<property  name="dtToOpsman" type="java.lang.String">
		<column name="DT_TO_OPSMAN"/>
		</property>
		<!-- End of Changes by <48206> for opsaman on 20/09/2013  -->
	</class>
	
	<query name="getNavigationDtlsByDrsNumber">
		<![CDATA[
				from com.ff.domain.delivery.DeliveryNavigatorDO drs WHERE drs.drsNumber IN (:drsNumber)
				AND drs.fromScreenCode=:fromScreenCode
				ORDER BY drs.transactionCreateDate DESC
		]]>
	</query>
	
	 <query name="getUnsyncInboundDrsNavigatorDtls">
		<![CDATA[
				from com.ff.domain.delivery.DeliveryNavigatorDO drs where (drs.dtToCentral='N' or drs.dtToCentral is null) 
		]]>
	</query> 

</hibernate-mapping>