<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>


	
	<class name="com.ff.domain.ratemanagement.operations.ratequotation.RateQuotationFixedChargesDO"  table="ff_f_rate_quotation_fixed_charges">
		<id name="rateQuotationFixedChargesId" type="java.lang.Integer" column="RATE_QUOTATION_FIXED_CHARGES_ID">
			<generator class="native" />
		</id>
		
		 <property name="value" >
        	<column name="COMPONENT_VALUE"/>
		</property>
		
		<many-to-one name="rateQuotationDO" class="com.ff.domain.ratemanagement.operations.ratequotation.RateQuotationDO"  lazy="false"  fetch="join">
		<column name="RATE_QUOTATION" />
		</many-to-one>
		 <property name="rateComponentCode" >
        	<column name="RATE_COMPONENT_CODE"/>
		</property>
	
		<property name="dtToBranch" column="DT_TO_BRANCH"/>  
       	<property name="createdDate" column="CREATED_DATE" />
 		<property name="createdBy" column="CREATED_BY" />
        <property name="updatedBy" column="UPDATE_BY" />
		<property name="updatedDate" column="UPDATE_DATE"/>
    </class>
    <query name="deleteFixedCharges">
		<![CDATA[
			DELETE FROM com.ff.domain.ratemanagement.operations.ratequotation.RateQuotationFixedChargesDO fixedChargesDO
			WHERE fixedChargesDO.rateQuotationDO.rateQuotationId =:rateQuotationId
		]]>
	</query>
</hibernate-mapping>