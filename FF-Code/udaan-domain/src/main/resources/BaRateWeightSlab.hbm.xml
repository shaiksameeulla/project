<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ff.domain.ratemanagement.operations.ba.BaRateWeightSlabDO" table="ff_f_rate_ba_weight_slab">
		<id name="baWeightSlabId">
			<column name="BA_WEIGHT_SLAB_ID" />
			<generator class="native" />
		</id>
		
		<property name="slabOrder" column="SLAB_ORDER" />
		
		<many-to-one name="baRateConfigProductHeaderDO" class="com.ff.domain.ratemanagement.operations.ba.BaRateConfigProductHeaderDO" fetch="select" lazy="false">
			<column name="BA_RATE_PRODUCT_HEADER"/>
		</many-to-one> 
		
		<many-to-one name="startWeight" class="com.ff.domain.ratemanagement.masters.WeightSlabDO" lazy="false">
			<column name="START_WEIGHT"/>
		</many-to-one> 
		
		<many-to-one name="endWeight" class="com.ff.domain.ratemanagement.masters.WeightSlabDO" lazy="false">
			<column name="END_WEIGHT"/>
		</many-to-one> 
		
		<set name="baSlabRateDO" table="ff_f_rate_ba_slab_rate"
			lazy="false" cascade="all-delete-orphan" inverse="false">
			<key column="BA_WEIGHT_SLAB" />
			<one-to-many
				class="com.ff.domain.ratemanagement.operations.ba.BaRateConfigSlabRateDO" />
		</set>
		
		<set name="baSpecialDestinationRateDO"
			table="ff_f_rate_ba_special_destination" cascade="all-delete-orphan" lazy="false" inverse="false">
			<key column="BA_WEIGHT_SLAB" />
			<one-to-many
				class="com.ff.domain.ratemanagement.operations.ba.BARateConfigSpecialDestinationRateDO" />
		</set>
		<!-- all-delete-orphan -->
	</class>
</hibernate-mapping>
	