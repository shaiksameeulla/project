	<query name="getSapCustomerDataByDateAndInterface">
		 <![CDATA[ 
		 		FROM com.ff.domain.ratemanagement.masters.SAPCustomerDO sapCustomer 
				WHERE sapCustomer.sapInbound = :status
				and sapCustomer.createdDate between :effectiveFromDate and :effectiveToDate
		 ]]>
	</query>		
	
</hibernate-mapping>