<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.ff.domain.ratemanagement.masters.WeightSlabDO" table="ff_d_weight_slab">
		<id name="weightSlabId" type="java.lang.Integer" column="WEIGHT_SLAB_ID">
			<generator class="native" />
		</id>
				
        <property name="startWeight" type="java.lang.Double" >
        <column name="START_WEIGHT" />
        </property>
        
        <property name="endWeight" type="java.lang.Double" >
        <column name="END_WEIGHT" />
        </property>
        
        <property name="additional" type="java.lang.String" >
        <column name="ADDITIONAL" />
        </property>
        
        <property name="effectiveFrom"  column="EFFECTIVE_FROM"/>
        <property name="effectiveTo"  column="EFFECTIVE_TO" />
        <property name="startWeightLabel"  column="START_WEIGHT_LABEL" />
        <property name="endWeightLabel"  column="END_WEIGHT_LABEL" />
        <property name="sequenceOrder"  column="SEQUENCE_ORDER" />
        <property name="weightSlabCategory" column="WEIGHT_SLAB_CATEGORY"/>
        <property name="dtToBranch" column="DT_TO_BRANCH"/>
    </class>
    
<query name="getWeightSlabByWtSlabCate">
	<![CDATA[ 
		FROM com.ff.domain.ratemanagement.masters.WeightSlabDO wtSlab
		WHERE wtSlab.weightSlabCategory=:weightSlabCategory
	]]>
</query>

<query name="getWeightSlabByWtSlabCateAndEndWt">
	<![CDATA[ 
		FROM com.ff.domain.ratemanagement.masters.WeightSlabDO wtSlab
		WHERE wtSlab.weightSlabCategory=:weightSlabCategory 
		AND wtSlab.endWeight=:endWeight
	]]>
</query>

</hibernate-mapping>