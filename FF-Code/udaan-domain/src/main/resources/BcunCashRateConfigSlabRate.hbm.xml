<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ff.domain.ratemanagement.operations.cash.BcunCashRateSlabRateDO" table="ff_f_rate_cash_slab_rate">
		<id name="slabRateId">
			<column name="CASH_SLAB_RATE_ID" />
			<generator class="assigned" />
		</id>
		<property name="rateHeaderProductDO">
            <column name="RATE_CASH_PRODUCT_MAP" />
        </property>
		<property name="originSectorId">
            <column name="ORIGIN_SECTOR" />
        </property>
		<property name="destinationSectorId">
			<column name="DEST_SECTOR" />
		</property>
        <property name="slabRate">
			<column name="SLAB_RATE" />
		</property>
		<property name="servicedOn">
			<column name="SERVICED_ON" />
		</property>
		<property name="weightSlabDO" column="CASH_WEIGHT_SLAB" />
        <property name="dtToBranch" column="DT_TO_BRANCH" />
		
	</class>
</hibernate-mapping>
