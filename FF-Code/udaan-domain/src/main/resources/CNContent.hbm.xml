<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.serviceOffering.CNContentDO" table="ff_d_cn_content">
		<id name="cnContentId" column="CN_CONTENT_ID" type="java.lang.Integer" >
			<generator class="assigned" />
		</id>
		
		<property name="cnContentCode"   type="java.lang.String" >
		<column name="CN_CONTENT_CODE" />
		</property>
		
		<property  name="cnContentName" 	type="java.lang.String">
		<column name="CN_CONTENT_NAME" />
		</property>
		<property  name="cnContentDesc" 	type="java.lang.String">
		<column name="CN_CONTENT_DESC" />
		</property>
		<property  name="status"  type="java.lang.String">
		<column name="STATUS" />
		</property>
		<property name="dtToBranch">
			<column name="DT_TO_BRANCH" />
		</property>	
	</class>
	
	<query name="getContentValues">
	<![CDATA[ FROM com.ff.domain.serviceOffering.CNContentDO content ]]>
	</query>
</hibernate-mapping>