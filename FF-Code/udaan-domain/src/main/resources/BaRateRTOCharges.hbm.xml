<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.ff.domain.ratemanagement.operations.ratecalculation.BARateRTOChargesDO"
		table="ff_f_rate_ba_rto_charge">
		<id name="rateBARTOChargesId" type="java.lang.Integer"
			column="RATE_BA_RTO_CHARGES_ID">
			<generator class="native" />
		</id>
		<property name="rtoChargeApplicable" column="RTO_CHARGE_APPLICABLE" />
		<property name="sameAsSlabRate" column="SAME_AS_SLAB_RATE" />
		<property name="discountOnSlab" column="DISCOUNT_ON_SLAB" />
		<property name="rateComponentCode" column="RATE_COMPONENT_CODE" />
		<property name="baRateHeaderId" column="BA_RATE_HEADER" />
		<property name="priorityInd" column="PRIORITY_IND" />
		<property name="dtToBranch" column="DT_TO_BRANCH" />
	</class>
	
	<query name="getRTOChargesForBA">
		<![CDATA[
			 FROM 	 com.ff.domain.ratemanagement.operations.ratecalculation.BARateRTOChargesDO baRateRTOChargesDO
			 WHERE 	 baRateRTOChargesDO.baRateHeaderId =:header
			 AND	 baRateRTOChargesDO.priorityInd =:priorityInd
		]]>
	</query>
</hibernate-mapping>