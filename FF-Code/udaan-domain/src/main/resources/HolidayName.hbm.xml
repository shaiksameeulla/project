<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ff.domain.masters.HolidayNameDO" table="ff_d_holiday_name">
		<id name="id" type="java.lang.Integer">
			<column name="HOLIDAY_NAME_ID" />
			<generator class="native" />
		</id>
		<property name="holidayName" type="string">
			<column name="HOLIDAY_NAME" length="50" />
		</property>
		<property name="holidayDescription" type="string">
			<column name="HOLIDAY_DESCRIPTION" length="100" />
		</property>
	</class>
</hibernate-mapping>
