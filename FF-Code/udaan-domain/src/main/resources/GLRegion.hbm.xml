<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
	<class name="com.ff.domain.mec.GLRegionDO" table="ff_d_gl_region">
		<id name="glRegionId" type="java.lang.Integer">
			<column name="GL_REGION_ID" />
			<generator class="native" />
		</id>

		<!-- Many -to- One relationships START -->
        <many-to-one name="glMasterDO"
			class="com.ff.domain.mec.GLMasterDO" lazy="false" fetch="join">
			<column name="GL_ID" />
		</many-to-one>

        <many-to-one name="regionDO"
			class="com.ff.domain.geography.RegionDO" lazy="false" fetch="join">
			<column name="REGION_ID" />
		</many-to-one>
		<!-- Many -to- One relationships END -->
		<property name="dtToBranch" column="DT_TO_BRANCH" />
	</class>
</hibernate-mapping>