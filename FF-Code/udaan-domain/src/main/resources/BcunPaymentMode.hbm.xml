<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">




<hibernate-mapping>
	<class name="com.ff.domain.serviceOffering.BcunPaymentModeDO" table="ff_d_payment_mode">
		<id name="paymentId" type="java.lang.Integer">
			<column name="PAYMENT_ID" />
			<generator class="assigned" />
		</id>
		<property name="paymentCode" type="java.lang.String">
			<column name="PAYMENT_CODE" />
		</property>
		<property name="paymentType" type="java.lang.String">
			<column name="PAYMENT_TYPE" />
		</property>
		<property name="description" type="java.lang.String">
			<column name="DESCRIPTION" />
		</property>
		<property name="status" type="java.lang.String">
			<column name="STATUS" />
		</property>
		<property name="dtToBranch">
			<column name="DT_TO_BRANCH" />
		</property>	
		</class>
		
</hibernate-mapping>