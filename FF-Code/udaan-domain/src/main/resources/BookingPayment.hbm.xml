<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.booking.BookingPaymentDO" table="ff_f_booking_payment">
		<id name="bookingPaymentId" column="BOOKING_PAYMENT_ID" type="java.lang.Integer" >
			<generator class="native" />
		</id>
		
		<property  name="chqDate" 	type="java.util.Date">
		<column name="CHQ_DATE" />
         </property>
		
		<property  name="paymentModeId" type="java.lang.Integer">
		<column name="PAYMENT_MODE" />
         </property>
	
		
		<property  name="chqNo" 	type="java.lang.String">
		<column name="CHQ_NO" />
         </property>
         
		<property  name="bankName" 	type="java.lang.String">
		<column name="BANK_NAME" />
         </property>
         
         <property  name="bankBranchName" 	type="java.lang.String">
		<column name="BANK_BRANCH_NAME" />
         </property>
         
         
	</class>

</hibernate-mapping>