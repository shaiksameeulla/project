<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ff.domain.stockmanagement.masters.StockStandardTypeDO" table="ff_d_std_type">
		<id name="id" type="java.lang.Integer">
			<column name="ID" />
			<generator class="assigned" />
		</id>
		<property name="stdTypeCode" column="STD_TYPE_CODE" />
		<property name="description" column="DESCRIPTION" />
		<property name="parentType" column="PARENT_TYPE" />
		<property name="typeName" column="TYPE_NAME" />
		<property name="activeStatus" column="CUR_STATUS" />
		<property name="dtToBranch">
			<column name="DT_TO_BRANCH" />
		</property>
		
	</class>
	
	<query name="getStockStdTypeByTypeName">
		<![CDATA[
 			FROM com.ff.domain.stockmanagement.masters.StockStandardTypeDO  typeDo 
 			where  typeDo.typeName IN (:typeName)
		]]>
	</query>	
	
	<query name="getStockStdTypeByTypeNameAsMap">
		<![CDATA[ select  new map(typeDo.stdTypeCode as typeId,typeDo.description as typeName)
 			FROM com.ff.domain.stockmanagement.masters.StockStandardTypeDO  typeDo 
 			where  typeDo.typeName IN (:typeName)
		]]>
	</query>	
	
	<query name="getTypeName">
		<![CDATA[
 			FROM com.ff.domain.stockmanagement.masters.StockStandardTypeDO  typeDo 
 			where  typeDo.typeName=:type
		]]>
	</query>	
	
	
	<query name="getManifestType">
		<![CDATA[
 			FROM com.ff.domain.stockmanagement.masters.StockStandardTypeDO  typeDo 
 			where  typeDo.typeName=:Type
		]]>
	</query>
	
	<query name="getLiabilityPaymentMode">
		<![CDATA[
 			FROM com.ff.domain.stockmanagement.masters.StockStandardTypeDO  typeDo 
 			where  typeDo.typeName=:typeName and typeDo.parentType=:parentType
		]]>
	</query>
	
	<query name="getBillingCycleBySTDCode">
		<![CDATA[
 			FROM	com.ff.domain.stockmanagement.masters.StockStandardTypeDO  stdTypeDo 
 			WHERE	stdTypeDo.stdTypeCode=:stdTypeCode
		]]>
	</query>
	
	<query name="getStdDetls">
		<![CDATA[
 			FROM	com.ff.domain.stockmanagement.masters.StockStandardTypeDO  stdTypeDo 
 			WHERE	stdTypeDo.id=:id
		]]>
	</query>
</hibernate-mapping>