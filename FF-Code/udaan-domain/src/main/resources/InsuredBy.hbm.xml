<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.ff.domain.serviceOffering.InsuredByDO" table="ff_d_insured_by">
		<id name="insuredById" type="java.lang.Integer">
			<column name="INSURED_BY_ID" />
			<generator class="native" />
		</id>
		
  		<property name="insuredByCode" type="java.lang.String">
			<column name="INSURED_BY_CODE" />
		</property>
		
		<property name="insuredByDesc" type="java.lang.String">
			<column name="INSURED_BY_DESC" />
		</property>
	<property name="percentile" type="java.lang.Double">
			<column name="PERCENTILE" />
		</property> 
		<property name="dtToBranch">
			<column name="DT_TO_BRANCH" />
		</property>	
		</class>		
		<query name="getInsuredBy">
	       <![CDATA[FROM com.ff.domain.serviceOffering.InsuredByDO insurance]]>
	    </query> 
		
	</hibernate-mapping>