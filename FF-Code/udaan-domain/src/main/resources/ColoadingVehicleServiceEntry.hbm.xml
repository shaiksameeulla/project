<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ff.domain.coloading.ColoadingVehicleServiceEntryDO" table="ff_d_coloading_vehicle_service_entry">
        <id name="id" type="java.lang.Integer">
            <column name="ID" />
            <generator class="native" />
        </id>
        <property name="date" type="timestamp">
            <column name="DATE" length="19" not-null="true" />
        </property>
        <property name="vehNumber" type="string">
            <column name="VEH_NUMBER" length="15" not-null="true" />
        </property>
        <property name="isRateCalculated" type="string">
            <column name="IS_RATE_CALCULATED"  length="2" not-null="true" />
        </property>
        <property name="ot" type="java.lang.Integer">
            <column name="OT" />
        </property>
        <property name="openingKm" type="integer">
            <column name="OPENING_KM" not-null="true" />
        </property>
        <property name="closingKm" type="integer">
            <column name="CLOSING_KM" not-null="true" />
        </property>
         <property name="officeId" type="integer">
            <column name="OFFICE_ID" not-null="true" />
        </property>
        <property name="createdBy" type="integer">
            <column name="CREATED_BY" not-null="true" />
        </property>
        <property name="updatedBy" type="integer">
            <column name="UPDATED_BY" not-null="true" />
        </property>
        <property name="createdDate" type="timestamp">
            <column name="CREATED_DATE" length="19" not-null="true" />
        </property>
        <property name="updatedDate" type="timestamp">
            <column name="UPDATED_DATE" length="19" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
