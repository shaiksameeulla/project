		<set name="rateQuotationWeightSlabDO" table = "ff_f_rate_quotation_weight_slab"  lazy="false" cascade="all-delete-orphan"  fetch="join">
		 
		<key column="RATE_QUOTATION_PRODUCT_CATEGORY_HEADER" />
		<one-to-many class="com.ff.domain.ratemanagement.operations.ratequotation.BcunRateQuotationWeightSlabDO" />
		 
		</set>
      
		<property name="dtToBranch" column="DT_TO_BRANCH"/>
