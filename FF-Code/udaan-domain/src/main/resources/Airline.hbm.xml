<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.transport.AirlineDO" table="ff_d_airline">
		<id name="airlineId" type="java.lang.Integer" column="AIRLINE_ID">
			<generator class="native" />
		</id>
		
		<property name="airlineCode" column="AIRLINE_CODE" />
		<property name="airlineName" column="AIRLINE_NAME" />
		<property name="recordStatus" column="RECORD_STATUS" />
		<property name="dtToBranch" column="DT_TO_BRANCH" />
	</class>
	
	
</hibernate-mapping>