<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.organization.SAPOfficeDO" table="ff_f_sap_office">
		<id name="id" type="java.lang.Integer">
			<column name="ID" />
			<generator class="native"/>
		</id>
		
		<property name="officeCode" column = "OFFICE_CODE" />
		<property name="officeName" column = "OFFICE_NAME" />
		<property name="address1" column = "ADDRESS_1" />
		<property name="address2" column = "ADDRESS_2" />
		<property name="address3" column = "ADDRESS_3" />
		<property name="officeTypeCode" column="OFFICE_TYPE_CODE"/>
		<property name="reportingRHO" column="REPORTING_RHO"/>
		<property name="reportingHUB" column="REPORTING_HUB"/>
		<property name="empCode" column="EMP_CODE"/>
		<property name="empName" column="EMP_NAME"/>
		<property name="virtualEmployee" column="EMPLOYEE_VIRTUAL"/>
		<property name="sapStatus" column="DT_SAP_INBOUND" />
		<property name="sapTimestamp" column="SAP_TIMESTAMP" />
		<property name="exception" column="EXCEPTION" />
		<property name="email" column = "EMAIL"/>
		<property name="pincode" column = "PINCODE" />
		<property name="createdBy" column="CREATED_BY" />
		<property name="updatedBy" column="UPDATE_BY" />
		<property name="createdDate" column="CREATED_DATE" />
		<property name="updatedDate" column="UPDATE_DATE" />		
		
	</class>
	
</hibernate-mapping>