<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
                      

<hibernate-mapping>
	<class
		name="com.ff.domain.ratemanagement.operations.ratequotation.RateQuotationRTOChargesDO"
		table="ff_f_rate_quotation_rto_charges">
		<id name="rateQuotationRTOChargesId" type="java.lang.Integer"
			column="RATE_QUOTATION_RTO_CHARGES_ID">
			<generator class="native" />
		</id>
		<property name="rtoChargeApplicable" column="RTO_CHARGE_APPLICABLE" />
		<property name="sameAsSlabRate" column="SAME_AS_SLAB_RATE" />
		<property name="discountOnSlab" column="DISCOUNT_ON_SLAB" />
		<property name="rateComponentDO" column="RATE_COMPONENT_CODE" />
		
		<many-to-one name="rateQuotationDO" class="com.ff.domain.ratemanagement.operations.ratequotation.RateQuotationDO"  lazy="false"  fetch="join">
			<column name="RATE_QUOTATION" />
		</many-to-one>
		<property name="dtToBranch" column="DT_TO_BRANCH"/>
		<property name="createdDate" column="CREATED_DATE" />
 		<property name="createdBy" column="CREATED_BY" />
        <property name="updatedBy" column="UPDATE_BY" />
		<property name="updatedDate" column="UPDATE_DATE"/>
	</class>
	
	<query name="deleteRTOCharges">
		<![CDATA[
			DELETE FROM com.ff.domain.ratemanagement.operations.ratequotation.RateQuotationRTOChargesDO rtoChargesDO
			WHERE rtoChargesDO.rateQuotationDO.rateQuotationId =:rateQuotationId
		]]>
	</query>
	
	<query name="getRTOChargesForRateQuotation">
		<![CDATA[
			 SELECT rateQuotationRTOChargesDO
			 FROM  com.ff.domain.ratemanagement.operations.ratequotation.RateQuotationRTOChargesDO rateQuotationRTOChargesDO
			 INNER JOIN rateQuotationRTOChargesDO.rateQuotationDO rateQuotationDO
			 WHERE rateQuotationDO.rateQuotationId =:rateQuotationId
		]]>
	</query>



</hibernate-mapping>