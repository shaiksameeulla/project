<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.leads.PlanFeedbackDO" table="ff_f_plan_feedback">
		<id name="planFeedbackId" column="PLAN_FEEDBACK_ID" type="java.lang.Integer" >
			<generator class="native" />
		</id>
		<property  name="feedbackCode" column="FEEDBACK_CODE" />
		<property  name="visitDate" column="VISIT_DATE"	type="java.util.Date" />	
		<property  name="purposeOfVisit" column="PURPOSE_OF_VISIT"	/>	
		<property  name="contactPerson" column="CONTACT_PERSON"	/>
		<property  name="time" column="TIME"	/>
		<many-to-one name="leadDO" class="com.ff.domain.leads.LeadDO"  lazy="false" fetch="join">
			<column name="LEAD_ID" />
		</many-to-one>
		<property  name="remarks" column="REMARKS"/>
		
		<many-to-one name="createdByEmployeeDO" class="com.ff.domain.organization.EmployeeDO" lazy="false"  fetch="join" update="false">
			<column name="CREATED_BY" />
		</many-to-one>
				
		<property name="createdDate"  column="CREATED_DATE" type="java.util.Date" update="false"/>
		
		<many-to-one name="updatedByEmployeeDO"  class="com.ff.domain.organization.EmployeeDO" lazy="false" fetch="join">
			<column name="UPDATED_BY" />
		</many-to-one>
		
		<property  name="updatedDate" column="UPDATED_DATE"	type="java.util.Date"/>
		
	</class>
	
	<query name="getLeadsPlanningFeedbackDtlsbyLeadNumberInTimeDescOrder">
		<![CDATA[  From com.ff.domain.leads.PlanFeedbackDO planFeedback 
		where  planFeedback.leadDO.leadNumber = :leadNumber 
		Order by planFeedback.visitDate desc ]]>
	</query>
	
	
</hibernate-mapping>