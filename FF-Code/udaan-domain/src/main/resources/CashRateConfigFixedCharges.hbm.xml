<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ff.domain.ratemanagement.operations.cash.CashRateConfigFixedChargesDO"
		table="ff_f_rate_cash_fixed_charges_config">
		<id name="cashFixedChargesConfigId" type="java.lang.Integer"
			column="CASH_FIXED_CHARGES_CONFIG_ID">
			<generator class="native" />
		</id>
		<property name="octroiBourneBy">
			<column name="OCTROI_BOURNE_BY" />
		</property>
		<property name="insuredBy">
			<column name="INSURED_BY" />
		</property>
		<!-- <property name="productMapId">
			<column name="CASH_PRODUCT_HEADER" />
		</property> -->
		<many-to-one name="cashRateHeaderDO" lazy="false"  fetch="join"
			class="com.ff.domain.ratemanagement.operations.cash.CashRateConfigHeaderDO">
			<column name="CASH_RATE_HEADER" />
		</many-to-one>
		<property name="dtToBranch" column="DT_TO_BRANCH" />
		<property name="priorityInd">
			<column name="PRIORITY_IND" />
		</property>
	</class>
	
	<query name="getCashRateFixedChrgsDtls">
		<![CDATA[ 
			FROM com.ff.domain.ratemanagement.operations.cash.CashRateConfigFixedChargesDO fixedChrgs
			WHERE fixedChrgs.cashRateHeaderDO.cashRateHeaderId=:productMapId
		]]>
	</query>
	<query name="deleteCashRateFixedChrgDtls">
		<![CDATA[
			DELETE FROM com.ff.domain.ratemanagement.operations.cash.CashRateConfigFixedChargesDO fixed
			WHERE fixed.cashRateHeaderDO.cashRateHeaderId IN (:productMapIds)
		]]>
	</query>
</hibernate-mapping>