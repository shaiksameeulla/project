<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.complaints.ServiceRequestTransferDO" table="ff_d_service_request_transfer_to">
		<id name="serviceRequestTransferToId" type="java.lang.Integer">
			<column name="SERVICE_REQUEST_TRANSFER_TO_ID" />
			<generator class="native" />
		</id>
		<property name="transferToCode" type="java.lang.String">
			<column name="TRANSFER_TO_CODE" />
		</property>
		<property name="transferToName" type="java.lang.String">
			<column name="TRANSFER_TO_NAME" />
		</property>
		<property name="transferToDescription" type="java.lang.String">
			<column name="TRANSFER_TOS_DESCRIPTION" />
		</property>
	</class>
		 	
</hibernate-mapping>