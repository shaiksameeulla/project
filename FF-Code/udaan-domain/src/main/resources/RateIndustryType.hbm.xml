<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ff.domain.ratemanagement.masters.RateIndustryTypeDO" table="ff_d_rate_industry_type">
		<id name="rateIndustryTypeId" type="java.lang.Integer">
			<column name="RATE_INDUSTRY_TYPE_ID" />
			<generator class="native" />
		</id>
		<property name="rateIndustryTypeCode" column="INDUSTRY_TYPE_CODE" />
		<property name="rateIndustryTypeName" column="INDUSTRY_TYPE_NAME" />
		<property name="dtToBranch" column="DT_TO_BRANCH"/>
	</class>
	
	
	<query name="getRateIndustryTypeByCode">
	<![CDATA[ 
		FROM	com.ff.domain.ratemanagement.masters.RateIndustryTypeDO rateIndustryTypeDO
		WHERE	rateIndustryTypeDO.rateIndustryTypeCode=:rateIndustryTypeCode
	 ]]>
	 </query>
	 
	 <query name="getRateIndustryType">
	<![CDATA[ 
		FROM	com.ff.domain.ratemanagement.masters.RateIndustryTypeDO rateIndustryTypeDO
		ORDER BY rateIndustryTypeDO.rateIndustryTypeName
	 ]]>
	 </query>

</hibernate-mapping>