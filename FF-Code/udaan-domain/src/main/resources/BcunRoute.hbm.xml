<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.routeserviced.BcunRouteDO" table="ff_d_route">
		<id name="routeId" type="java.lang.Integer" column="ROUTE_ID">
			<generator class="assigned" />
		</id>
		
        <property name="distance" type="java.lang.Double" column="DISTANCE" />		
        <property name="originCityId" column="ORIGIN_CITY" type="java.lang.Integer"/>
        <property name="destCityId" column="DEST_CITY" type="java.lang.Integer"/>
        
		<property name="dtToBranch">
			<column name="DT_TO_BRANCH" />
		</property>	
		
	</class>
</hibernate-mapping>