<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ff.domain.ratemanagement.operations.ba.BaSpecialDestinationRateDO" table="ff_f_rate_ba_special_destination">
		<id name="specialDestinationId">
			<column name="RATE_BA_SPECIAL_DEST_ID" />
			<generator class="native" />
		</id>

		<property name="rateProductHeaderId">
            <column name="BA_RATE_PRODUCT_HEADER" />
         </property>
        <many-to-one name="weightSlab" class="com.ff.domain.ratemanagement.operations.ba.BaRateCalculationWeightSlabDO" lazy="false">
           <column name="BA_WEIGHT_SLAB"/>
        </many-to-one>
        <property name="stateId">
            <column name="STATE_ID" />
        </property>
        <property name="cityId">
            <column name="CITY" />
        </property>
        <!-- <property name="pincode">
            <column name="PINCODE" />
        </property> -->
		<property name="rate">
			<column name="SLAB_RATE" />
		</property>
		<property name="servicedOn">
			<column name="SERVICED_ON" />
		</property>
		<property name="dtToBranch" column="DT_TO_BRANCH" />
	</class>
	
<!-- 	<query name="getBASpecialDestinationSlabs">
		<![CDATA[ 
			FROM   com.ff.domain.ratemanagement.operations.ba.BaSpecialDestinationRateDO specialDest 
			WHERE  specialDest.rateProductHeaderId=:baRateHeaderId 
			AND    specialDest.cityId=:cityId
			AND	   specialDest.servicedOn =:servicedOn
		]]>
	</query> -->
</hibernate-mapping>
	