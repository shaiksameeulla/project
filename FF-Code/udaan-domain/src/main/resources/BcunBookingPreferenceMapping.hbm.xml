<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.booking.BcunBookingPreferenceMappingDO" table="ff_f_booking_preference_mapping">
		<id name="bookingPreId" column="BOOKING_PRE_ID" type="java.lang.Integer" >
			<generator class="native" />
		</id>
		
		<property name="bookingId"  type="java.lang.Integer"  >
		<column name="BOOKING_ID" />
        </property> 
		
		<property name="referenceId" type="java.lang.Integer"  >
		<column name="PREFERENCE_ID" />
         </property>
		
		<property  name="splInstructions" 	type="java.lang.String" >
		<column name="SPL_INSTRUCTIONS" />
         </property>
		
	</class>
	 
	 <query name="getBookingPrefMappingDtls">
		<![CDATA[
			 from com.ff.domain.booking.BcunBookingPreferenceMappingDO booking where booking.bookingId = :bookingId AND booking.referenceId = :referenceId ]]>
	</query>

</hibernate-mapping>