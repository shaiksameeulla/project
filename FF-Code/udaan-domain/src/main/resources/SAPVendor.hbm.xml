<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.business.SAPVendorDO" table="ff_f_sap_vendor">
		<id name="id" type="java.lang.Integer" column="ID">
			<generator class="increment" />
		</id>
		
        <property name="vendorCode" column="VENDOR_CODE" />
        <property name="firstname" column="FIRST_NAME" />
        <property name="lastName" column="LAST_NAME" />
        <property name="vendorType" column="VENDOR_TYPE" />
        <property name="address" column="ADDRESS" />
        <property name="companyType" column="COMPANY_TYPE" />
        <property name="email" column="EMAIL" />
        <property name="fax" column="FAX" />
        <property name="businessName" column="BUSINESS_NAME" />
        <property name="mobile" column="MOBILE" />
        <property name="phone" column="PHONE" />
        <property name="pincode" column="PINCODE" />
        <property name="service" column="SERVICE" />
        <property name="rhocode" column="RHO_CODE" />
        <property name="exception" column="EXCEPTION" />
        <property name="sapStatusInBound" column="DT_SAP_INBOUND" />
        <property name="sapTimestamp" column="SAP_TIMESTAMP" />
        <property name="createdDate" column="CREATED_DATE" update="false"/>
   </class>
	
</hibernate-mapping>