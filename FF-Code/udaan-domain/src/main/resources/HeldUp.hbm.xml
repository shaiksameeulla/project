<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.heldup.HeldUpDO" table="ff_f_heldup">
		<id name="heldUpId" type="java.lang.Integer" column="HELDUP_ID">
			<generator class="native" />
		</id>
		
        <property name="heldUpNumber" column="HELDUP_NUMBER"/>
        <property name="transactionType" column="TRANSACTION_TYPE"/>
        <property name="transactionNumber" column="TRANSACTION_NUMBER"/>
        <property name="currentLocation" column="CURRENT_LOCATION"/>
        <property name="remarks" column="REMARKS"/>
        <property name="processNumber" column="PROCESS_NUMBER"/>
        <property name="heldUpDate" column="HELDUP_DATE" type="java.util.Date" />
	
        <property name="dtToBranch" column="DT_TO_BRANCH"/>
        <property name="dtToCentral" column="DT_TO_CENTRAL"/>
        <property name="dtFromOpsman" column="DT_FROM_OPSMAN"/>
        <property name="dtUpdateToCentral" column="DT_UPDATE_TO_CENTRAL"/>
		
		<property name="createdDate"  column="CREATED_DATE" type="java.util.Date" update="false"/>
		<property name="updatedDate" column="UPDATE_DATE"	type="java.util.Date"/>    
		<property name="createdBy" column="CREATED_BY" type="java.lang.Integer" update="false"/>
		<property name="updatedBy" column="UPDATE_BY" type="java.lang.Integer"/>
		
		<many-to-one name="officeDO" class="com.ff.domain.organization.OfficeDO" lazy="false" fetch="join">
			<column name="OFFICE" />
		</many-to-one>
		
		<many-to-one name="userDO" class="com.ff.domain.umc.UserDO" lazy="false" fetch="join">
			<column name="USER_ID"/>
		</many-to-one>	
					
		<many-to-one name="employeeDO" class="com.ff.domain.organization.EmployeeDO" lazy="false" fetch="join">
			<column name="EMPLOYEE"/>
		</many-to-one>
							
		<many-to-one name="reasonDO" class="com.ff.domain.serviceOffering.ReasonDO" lazy="false" fetch="join">
			<column name="REASON"/>
		</many-to-one>
		
		<many-to-one name="processDO"  class="com.ff.domain.tracking.ProcessDO" lazy="false" fetch="join">
			<column  name="UPDATED_PROCESS_FROM" />
		</many-to-one>
	</class>
		
	<query name="getHeldUpByHeldUpNoAndOfficeId">
		<![CDATA[	FROM com.ff.domain.heldup.HeldUpDO heldUp
					where heldUp.heldUpNumber=:heldUpNumber
					and heldUp.officeDO.officeId=:officeId
				]]>
	</query>
				
</hibernate-mapping>