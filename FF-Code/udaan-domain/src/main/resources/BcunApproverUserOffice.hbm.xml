<hibernate-mapping>
	<class name="com.ff.domain.umc.BcunApproverUserOfficeDO" table="ff_d_approver_user_office">
		<id name="approverUserOfficeId" type="java.lang.Integer">
			<column name="APPROVERUSEROFFICE_ID" />
			<generator class="assigned"/>
		</id>
		<property name="user" type="java.lang.Integer">
			<column name="USER_ID" />
		</property>
		<property name="office" type="java.lang.Integer">
			<column name="OFFICE" />
		</property>
		<property name="city" type="java.lang.Integer">
			<column name="CITY" />
		</property>	
		<property name="regionalOffice" type="java.lang.Integer">
			<column name="REGIONAL_OFFICE" />
		</property>	
		<property name="mappedTo" type="java.lang.String">
			<column name="MAPPED_TO" />
		</property>
		<property name="dtToBranch">
			<column name="DT_TO_BRANCH" />
		</property>	
				
	</class>
