<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.serviceOffering.RemarksDO" table="ff_d_remarks">
		<id name="remarkId" column="REMARK_ID" type="java.lang.Integer" >
			<generator class="assigned" />
		</id>
		<property name="remarkType" column="REMARK_TYPE"/>
		<property name="description" column="DESCRIPTION"/>	
		<property name="status" column="STATUS"/>	
		<property name="dtToBranch">
			<column name="DT_TO_BRANCH" />
		</property>	
	</class>
</hibernate-mapping>