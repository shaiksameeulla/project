<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.ff.domain.ratemanagement.operations.ratequotation.RateCalculationWeightSlabDO"
		table="ff_f_rate_quotation_weight_slab">
		<id name="rateQuotationWeightSlabId" type="java.lang.Integer"
			column="RATE_QUOTATION_WEIGHT_SLAB_ID">
			<generator class="native" />
		</id>

		<many-to-one name="startWeight"
			class="com.ff.domain.ratemanagement.masters.WeightSlabDO" lazy="false"  fetch="join">
			<column name="START_WEIGHT"></column>
		</many-to-one>

		<many-to-one name="endWeight"
			class="com.ff.domain.ratemanagement.masters.WeightSlabDO" lazy="false"  fetch="join">
			<column name="END_WEIGHT"></column>
		</many-to-one>

		<property name="order" column="SLAB_ORDER" />
	</class>
</hibernate-mapping>