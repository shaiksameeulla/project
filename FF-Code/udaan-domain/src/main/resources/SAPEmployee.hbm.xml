<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.ff.domain.organization.SAPEmployeeDO" table="ff_f_sap_employee">
	<id name="employeeId" type="java.lang.Integer">
			<column name="EMPLOYEE_ID" />
			<generator class="native" />
		</id>
		<property name="empCode" column="EMP_CODE" />
		<property name="firstName" column="FIRST_NAME" />
		<property name="lastName" column="LAST_NAME" />
		<property name="empPhone" column="PHONE" />
		<property name="designation" column="DESIGNATION" />
		<property name="empType" column="EMP_TYPE" />
		<property name="department" column="DEPARTMENT" />
		<property name="city" column="CITY" />
		<property name="office" column="OFFICE" />
		<property name="emailId" column="EMAIL_ID" />
		<property name="createdBy" column="CREATED_BY" />
		<property name="creationDate" column="CREATION_DATE" />
		<property name="updateBy" column="UPDATE_BY" />
		<property name="updateDate" column="UPDATE_DATE" />
		<property name="dtToBranch" column="DT_TO_BRANCH" />
		<property name="empStatus" column="EMP_STATUS" />
		</class>
</hibernate-mapping>