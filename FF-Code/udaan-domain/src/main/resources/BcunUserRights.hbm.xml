<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ff.domain.umc.BcunUserRightsDO" table="ff_d_user_rights">
		<id name="userRightsId" type="java.lang.Integer">
			<column name="USER_RIGHTS_ID" />
			<generator class="assigned" />
		</id>
		<property name="status" type="java.lang.String">
			<column name="STATUS" />
		</property>
		<property name="dtToBranch">
			<column name="DT_TO_BRANCH" />
		</property>
		<property name="userId">
			<column name="USER_ID" />
		</property>
		<property name="roleId">
			<column name="ROLE_ID" />
		</property>
		<property name="createdBy" column="CREATED_BY" />
		<property name="updatedBy" type="java.lang.Integer">
			<column name="UPDATE_BY" />
		</property>
		<property name="transactionCreateDate" type="java.util.Date">
			<column name="CREATION_DATE" />
		</property>
		<property name="transactionModifiedDate" type="java.util.Date">
			<column name="UPDATE_DATE" />
		</property>


	</class>

	<query name="getOutboundBranchRights">
	<![CDATA[ 
		select rights FROM com.ff.domain.umc.BcunUserRightsDO rights, com.ff.domain.umc.EmployeeUserDO empUser where 
		rights.userId = empUser.userId
		and  empUser.empDO.officeId=:branchId
		and  rights.dtToBranch ='N' 
	]]>
	</query>

</hibernate-mapping>
	