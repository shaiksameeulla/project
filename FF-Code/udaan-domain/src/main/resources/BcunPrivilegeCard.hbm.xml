<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.serviceOffering.BcunPrivilegeCardDO" table="ff_d_privilege_card">
		<id name="privilegeCardId" type="java.lang.Integer">
			<column name="PRIVILEGE_CARD_ID" />
			<generator class="assigned" />
		</id>
		<property name="customerId" column="CUSTOMER_ID" type="java.lang.Integer" />
		<property name="balance" column="BALANCE" type="java.lang.Double" />
		<property name="status" column="STATUS" type="java.lang.String"/>
		<property name="privilegeCardNo" column="PRIVILEGE_CARD_NO" type="java.lang.String"/>
		<property name="dtToBranch">
			<column name="DT_TO_BRANCH" />
		</property>	
	</class>
	
	
	
</hibernate-mapping>