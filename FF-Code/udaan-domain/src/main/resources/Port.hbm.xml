<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.transport.PortDO" table="ff_d_port">
		<id name="portId" type="java.lang.Integer" column="PORT_ID">
			<generator class="assigned" />
		</id>
		
        <property name="portName" type="java.lang.String" column="PORT_NAME" />
        <property name="portCode" type="java.lang.String" column="PORT_CODE" />
        <property name="portType" type="java.lang.String" column="PORT_TYPE" />
		
		<many-to-one name="cityDO" class="com.ff.domain.geography.CityDO" lazy="false" fetch="join">
			<column name="PORT_CITY" />
		</many-to-one>
				
		<many-to-one name="transportModeDO" class="com.ff.domain.transport.TransportModeDO" lazy="false" fetch="join">
			<column name="TRANSPORT_MODE" />
		</many-to-one>		
		
		<property name="dtToBranch">
			<column name="DT_TO_BRANCH" />
		</property> 
	</class>
</hibernate-mapping>