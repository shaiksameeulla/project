<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.consignment.DuplicateChildConsignmentDO" table="ff_f_child_consignments_duplicate">
		<id name="childCNId" type="java.lang.Integer" column="BOOKING_CHILD_CN_ID">
			<generator class="native" />
		</id>

		<property name="childConsgNumber" column="CHILD_CONSG_NUMBER"/>
		<property name="childConsgWeight" column="CHILD_CONSG_WEIGHT" type="java.lang.Double"/>
		<property name="duplicateConsignmentId" column="CONSG_ID" type="java.lang.Integer"/>
		
	</class>
</hibernate-mapping>