<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.ff.domain.ratemanagement.operations.ratebenchmark.BcunRateBenchMarkHeaderDO" table="ff_d_rate_bench_mark_header">
		<id name="rateBenchMarkHeaderId" type="java.lang.Integer" column="RATE_BENCH_MARK_HEADER_ID">
			<generator class="assigned" />
		</id>
				
        <property name="rateBenchMarkEffectiveFrom" column="RATE_BENCH_MARK_EFFECTIVE_FROM"/>
        <property name="rateBenchMarkEffectiveTo" column="RATE_BENCH_MARK_EFFECTIVE_TO"/>
        <property name="rateIndustryCategoryId" column="RATE_INDUSTRY_CAETGORY"/>
        <property name="rateCustomerCategoryId" column="RATE_CUSTOMER_CATEGORY"/>
        <property name="approver" column="APPROVER"/>
        
        <set name="rateBenchMarkProductDO" table = "ff_d_rate_bench_mark_product" lazy="false" cascade="all"  fetch="join">
		 
		<key column="RATE_BENCH_MARK_HEADER" />
		<one-to-many class="com.ff.domain.ratemanagement.operations.ratebenchmark.BcunRateBenchMarkProductDO" />
		 
		</set>
		
		<property name="isApproved" column="BENCH_MARK_APPROVED"/>
		<property name="dtToBranch" column="DT_TO_BRANCH"/>
    </class>
</hibernate-mapping>