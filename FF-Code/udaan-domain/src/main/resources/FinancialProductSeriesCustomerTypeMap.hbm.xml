<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.billing.FinancialProductCustTypeMapDO" table="ff_d_financial_product_series_customer_type_map">
		<id name="financialProductMapId" column="FINANCIAL_PRODUCT_MAP_ID" type="java.lang.Integer" >
			<generator class="native" />
		</id>
	   <many-to-one name="financialProductDO"
			class="com.ff.domain.billing.FinancialProductDO" lazy="false" fetch="join">
			<column name="FINANCIAL_PRODUCT" />
		</many-to-one>
		
		<many-to-one name="productDO"
			class="com.ff.domain.serviceOffering.ProductDO" lazy="false" fetch="join">
			<column name="PRODUCT" />
		</many-to-one>
		
		<many-to-one name="customerTypeDO"
			class="com.ff.domain.business.CustomerTypeDO" lazy="false" fetch="join">
			<column name="CUSTOMER_TYPE" />
		</many-to-one>
		
		<property  name="status" column="STATUS" type="java.lang.String"/>
		<property name="dtToBranch">
			<column name="DT_TO_BRANCH" />
		</property> 
	</class>
	
	
</hibernate-mapping>