<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ff.domain.ratemanagement.operations.cash.BcunCashRateSpecialDestinationDO" 
		table="ff_f_rate_cash_special_destination">
		<id name="specialDestId">
			<column name="RATE_CASH_SPECIA_DEST" />
			<generator class="assigned" />
		</id>
		<!-- <property name="pincode">
			<column name="PINCODE" />
		</property> -->
		<property name="stateId">
            <column name="STATE_ID" />
         </property>
		<property name="slabRate">
			<column name="SLAB_RATE" />
		</property>
		<property name="servicedOn">
			<column name="SERVICED_ON" />
		</property>
		<property name="weightSlab" column="CASH_WEIGHT_SLAB" />
		<property name="cityId" column="CITY" />
		<property name="rateHeaderProduct" column="RATE_CASH_PRODUCT_MAP" />
		<property name="dtToBranch" column="DT_TO_BRANCH" />
		<property name="originSector">
            <column name="ORIGIN_SECTOR" />
        </property>
	</class>
</hibernate-mapping>
	