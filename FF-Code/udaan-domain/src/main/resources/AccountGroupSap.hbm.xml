<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ff.domain.ratemanagement.masters.AccountGroupSapDO" table="ff_d_account_group_sap">
		<id name="accountGroupSapId" type="java.lang.Integer" column="ACCOUNT_GROUP_SAP_ID">
			<generator class="native" />
		</id>
		
        <property name="customerGroup" column="CUSTOMER_GROUP"/>
        <property name="accountAssignGroup" column="ACCOUNT_ASSIGN_GROUP"/>
         <property name="accountGroup" column="ACCOUNT_GROUP"/>
        
        
        <many-to-one name="rateCustomerCategoryId" class="com.ff.domain.ratemanagement.masters.RateCustomerCategoryDO" lazy="false" fetch="join">
			<column name="RATE_CUSTOMER_CATEGORY_ID" />
		</many-to-one>
        <property name="dtToBranch" column="DT_TO_BRANCH"/>
	</class>
	
	<query name="getRateCustCatByCustGroupCode">
		<![CDATA[
			FROM	com.ff.domain.ratemanagement.masters.AccountGroupSapDO accGrpSAP 
			WHERE	accGrpSAP.customerGroup=:customerGroup
		]]>
	</query>
	
	
</hibernate-mapping>