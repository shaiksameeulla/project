<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.routeserviced.RouteModeMappingDO" table="ff_d_route_mode_mapping">
		<id name="routeModeMappingId" type="java.lang.Integer" column="ROUTE_MODE_MAPPING_ID">
			<generator class="native" />
		</id>
		
        <many-to-one name="routeDO" class="com.ff.domain.routeserviced.RouteDO" lazy="false" fetch="join" cascade="all">
			<column name="ROUTE" />
		</many-to-one>
				
		<many-to-one name="transportModeDO" class="com.ff.domain.transport.TransportModeDO" lazy="false" fetch="join">
			<column name="TRANSPORT_MODE" />
		</many-to-one>
		
		<property name="dtToBranch">
			<column name="DT_TO_BRANCH" />
		</property> 
	</class>
</hibernate-mapping>