<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.complaints.ServiceRequestQueryTypeDO" table="ff_d_service_request_query_type">
		<id name="serviceRequestQueryTypeId" type="java.lang.Integer">
			<column name="SERVICE_REQUEST_QUERY_TYPE_ID" />
			<generator class="native" />
		</id>
		<property name="queryTypeCode" type="java.lang.String">
			<column name="QUERY_TYPE_CODE" />
		</property>
		<property name="queryTypeName" type="java.lang.String">
			<column name="QUERY_TYPE_NAME" />
		</property>
		<property name="queryTypeDescription" type="java.lang.String">
			<column name="QUERY_TYPE_DESCRIPTION" />
		</property>
		<property name="queryType" column="QUERY_TYPE"/>
		
	</class>
		 	
</hibernate-mapping>