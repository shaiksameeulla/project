<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ff.domain.ratemanagement.operations.ba.BaAdditionalChargesDO" table="ff_f_rate_ba_additional_charges">
		<id name="additionalChrgId">
			<column name="RATE_BA_ADDL_CHARGE_ID" />
			<generator class="native" />
		</id>
		<property name="componentValue">
            <column name="COMPONENT_VALUE" />
        </property>
		<property name="componentCode">
            <column name="RATE_COMPONENT_CODE" />
         </property>
        <property name="baRateheaderId">
            <column name="BA_RATE_HEADER" />
         </property>
		<property name="priorityInd">
			<column name="PRIORITY_IND" />
		</property>
	</class>
</hibernate-mapping>
	