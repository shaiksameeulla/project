<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.routeserviced.ServiceByTypeDO" table="ff_d_service_by_type">
		<id name="serviceByTypeId" type="java.lang.Integer" column="SERVICED_BY_TYPE_ID">
			<generator class="assigned" />
		</id>
		
        <property name="serviceByTypeCode" type="java.lang.String" column="SERVICED_BY_TYPE_CODE" />
        <property name="serviceByTypeDesc" type="java.lang.String" column="SERVICED_BY_TYPE_DESC" />
        	<property name="dtToBranch">
			<column name="DT_TO_BRANCH" />
		</property> 
        				
		<many-to-one name="transportModeDO" class="com.ff.domain.transport.TransportModeDO" lazy="false" fetch="join">
			<column name="TRANSPORT_MODE" />
		</many-to-one>		 	
	</class>
</hibernate-mapping>