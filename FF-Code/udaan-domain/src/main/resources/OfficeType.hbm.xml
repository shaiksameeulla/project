<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.organization.OfficeTypeDO" table="ff_d_office_type">
		<id name="offcTypeId" type="java.lang.Integer">
			<column name="OFFICE_TYPE_ID" />
			<generator class="assigned" />
		</id>
		<property name="offcTypeCode" type="java.lang.String">
			<column name="OFFICE_TYPE_CODE" />
		</property>

		<property name="offcTypeDesc" type="java.lang.String">
			<column name="OFFICE_TYPE_DESC" />
		</property>
		
		<property name="aaplicableForDispatch" type="java.lang.String">
			<column name="APPLICABLE_FOR_DISPATCH" />
		</property>
		<property name="dtToBranch">
			<column name="DT_TO_BRANCH" />
		</property>	
	</class>
	
	<query name="getOfficeTypeByOfficeTypeID"> 
		<![CDATA[	FROM com.ff.domain.organization.OfficeTypeDO officeType
					WHERE officeType.offcTypeCode=:offcTypeCode 
				]]>
	</query>
	
</hibernate-mapping>