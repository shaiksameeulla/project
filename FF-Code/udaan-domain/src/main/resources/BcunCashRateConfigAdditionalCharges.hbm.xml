<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ff.domain.ratemanagement.operations.cash.BcunCashRateConfigAdditionalChargesDO" 
		table="ff_f_rate_cash_additional_charges">
		<id name="rateCashAdditionalChrgId">
			<column name="RATE_CASH_ADDITIONAL_CHRG_ID" />
			<generator class="assigned" />
		</id>
		<property name="componentValue">
			<column name="COMPONENT_VALUE" />
		</property>
		<property name="componentCode">
			<column name="COMPONENT_CODE" />
		</property>
		<property name="priorityInd">
			<column name="PRIORITY_IND" />
		</property>
		<property name="cashRateHeaderDO">
			<column name="CASH_RATE_HEADER" />
		</property>
        <property name="dtToBranch" column="DT_TO_BRANCH" />
        
	</class>

</hibernate-mapping>
