<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.billing.ReBillingConsignmentDO" table="ff_f_re_billing_consignment">
		<id name="reBillingConsignmentId" type="java.lang.Integer" column="REBILLING_CONSIGNMENT_ID">
			<generator class="native" />
		</id>

		<property name="reBillId" column="RE_BILLING_ID" type="java.lang.Integer" />
		<property name="consgId" column="CONSG_ID" type="java.lang.Integer" />
		<property name="consgNo" column="CONSG_NO" />
		<property name="orgOffId" column="ORIGIN_OFFICE" type="java.lang.Integer" />
		<many-to-one  name="destPincodeDO"  class="com.ff.domain.geography.PincodeDO" lazy="false" fetch="join">
		<column  name="DEST_PINCODE" />
		</many-to-one>
		<property name="noOfPcs" column="NO_OF_PCS" type="java.lang.Integer" />
		<many-to-one name="consignmentTypeDO"
			class="com.ff.domain.serviceOffering.ConsignmentTypeDO" lazy="false" fetch="join">
			<column name="CONSG_TYPE" />
		</many-to-one>
		<property name="price" column="PRICE" type="java.lang.Double" />
		<many-to-one name="productDO"
			class="com.ff.domain.serviceOffering.ProductDO" lazy="false" fetch="join">
			<column name="PRODUCT" />
		</many-to-one>
		<property name="actualWeight" column="ACTUAL_WEIGHT" type="java.lang.Double" />
		<property name="volWeight" column="VOL_WEIGHT" type="java.lang.Double" />
		<property name="finalWeight" column="FINAL_WEIGHT" type="java.lang.Double" />
		<property name="dtToBranch" column="DT_TO_BRANCH" />
		<property name="dtToCentral" column="DT_TO_CENTRAL" />
		<property name="dtFromOpsman" column="DT_FROM_OPSMAN" />
		<many-to-one name="updatedProcessDO" class="com.ff.domain.tracking.ProcessDO"
			lazy="false" fetch="join">
			<column name="UPDATED_PROCESS_FROM" />
		</many-to-one>
		<property name="dtUpdateToCentral" column="DT_UPDATE_TO_CENTRAL" />
		<property name="mobileNo" column="MOBILE_NO" />
		<many-to-one name="cnContentDO"
			class="com.ff.domain.serviceOffering.CNContentDO" lazy="false" fetch="join">
			<column name="CONTENT" />
		</many-to-one>
		<many-to-one name="cnPaperWorksDO"
			class="com.ff.domain.serviceOffering.CNPaperWorksDO" lazy="false" fetch="join">
			<column name="PAPER_WORK" />
		</many-to-one>
		<many-to-one name="insuredByDO"
			class="com.ff.domain.serviceOffering.InsuredByDO" lazy="false" fetch="join">
			<column name="INSURED_BY" />
		</many-to-one>
		<property name="insurencePolicyNo" column="INSURANCE_POLICY_NO" />
		<property name="refNo" column="REF_NO" />
		<property name="height" column="HEIGHT" type="java.lang.Double" />
		<property name="length" column="LENGTH" type="java.lang.Double" />
		<property name="breath" column="BREADTH" type="java.lang.Double" />
		<property name="receivedDateTime" column="RECEIVED_DATE" type="java.util.Date" />
		<property name="deliveredDate" column="DLV_DATE_TIME" type="java.util.Date" />
		<property name="recvNameOrCompName" column="RECV_NAME_OR_COMP_SEAL" />
		<property name="receivedStatus" column="RECEIVED_STATUS" />
		<many-to-one name="consignor"
			class="com.ff.domain.business.ConsigneeConsignorDO" lazy="false" fetch="join">
			<column name="CONSIGNOR" />
		</many-to-one>

		<many-to-one name="consignee"
			class="com.ff.domain.business.ConsigneeConsignorDO" lazy="false" fetch="join">
			<column name="CONSIGNEE" />
		</many-to-one>
		<property name="dtToOpsman" column="DT_TO_OPSMAN" />
		<property name="createdDate" column="CREATED_DATE" type="java.util.Date" />
		<property name="updatedDate" column="UPDATE_DATE" type="java.util.Date" />
		<property name="createdBy" column="CREATED_BY" type="java.lang.Integer" />
		<property name="updatedBy" column="UPDATE_BY" type="java.lang.Integer" />
		<property name="declaredValue" column="DECLARED_VALUE" type="java.lang.Double" />
		<property name="consgStatus" column="CONSG_STATUS" />
		<property name="billingConsgId" column="BILLING_CONSIGNMENT" />
		
	</class>

   <query name="getTotalRebillCnCount">
		<![CDATA[
          
          SELECT COUNT(*) FROM com.ff.domain.billing.ReBillingConsignmentDO as rebillCn 
  	        WHERE  rebillCn.reBillId =:RebillConsgId 
         
             ]]>
	</query>
   


</hibernate-mapping>