<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.transport.TransportModeDO" table="ff_d_transport_mode">
		<id name="transportModeId" type="java.lang.Integer" column="TRANSPORT_MODE_ID">
			<generator class="assigned" />
		</id>
		
		<property name="transportModeCode" type="java.lang.String" column="TRANSPORT_MODE_CODE" />
		<property name="transportModeDesc" type="java.lang.String" column="TRANSPORT_MODE_DESCRIPTION" />    
				<property name="dtToBranch">
			<column name="DT_TO_BRANCH" />
		</property>	
	</class>
		
	<query name="getTransportModeByCode">
		<![CDATA[FROM com.ff.domain.transport.TransportModeDO transportMode 
				where transportMode.transportModeCode=:transportModeCode]]>
	</query>
	
</hibernate-mapping>