<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
	<class name="com.ff.domain.geography.BcunCountryDO" table="ff_d_country">
		<id name="countryId" column="COUNTRY_ID" type="java.lang.Integer">
			<generator class="assigned" />
		</id>
		<property name="countryCode" column="COUNTRY_CODE" />
		<property name="countryName" column="COUNTRY_NAME" />
		<property name="createdBy" column="CREATED_BY" />
		<property name="createdDate" column="CREATION_DATE" />
		<property name="updatedBy" column="UPDATE_BY" />
		<property name="updatedDate" column="UPDATE_DATE" />
		<property name="dtToBranch" column="DT_TO_BRANCH" />

	</class>

</hibernate-mapping>