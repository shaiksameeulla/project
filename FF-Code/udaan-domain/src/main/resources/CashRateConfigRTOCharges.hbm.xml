<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ff.domain.ratemanagement.operations.cash.CashRateConfigRTOChargesDO"
		table="ff_f_rate_cash_rto_charge">
		<id name="rateCashRTOChargesId" type="java.lang.Integer" column="RATE_CASH_RTO_CHARGES_ID">
			<generator class="native" />
		</id>
		<property name="rtoChargeApplicable" column="RTO_CHARGE_APPLICABLE" />
		<property name="sameAsSlabRate" column="SAME_AS_SLAB_RATE" />
		<property name="discountOnSlab" column="DISCOUNT_ON_SLAB" />
		<property name="rateComponentCode" column="RATE_COMPONENT_CODE" />
		<!-- <property name="productMapId" column="RATE_CASH_PRODUCT_MAP_ID" /> -->
		<property name="priorityInd" column="PRIORITY_IND" />
		<many-to-one name="cashRateHeaderDO" lazy="false"  fetch="join"
			class="com.ff.domain.ratemanagement.operations.cash.CashRateConfigHeaderDO">
			<column name="CASH_RATE_HEADER" />
		</many-to-one>
		<property name="dtToBranch" column="DT_TO_BRANCH" />
	</class>
	
	<query name="getCashRateRtoChrgsDtls">
		<![CDATA[ 
			FROM com.ff.domain.ratemanagement.operations.cash.CashRateConfigRTOChargesDO rtoChrgs
			WHERE rtoChrgs.cashRateHeaderDO.cashRateHeaderId=:productMapId
		]]>
	</query>
	<query name="deleteCashRateRTOChrgDtls">
		<![CDATA[
			DELETE FROM com.ff.domain.ratemanagement.operations.cash.CashRateConfigRTOChargesDO rto
			WHERE rto.cashRateHeaderDO.cashRateHeaderId IN (:productMapIds)
		]]>
	</query>
</hibernate-mapping>