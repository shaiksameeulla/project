<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.masters.ConsignmentStatusSmsDO" table="ff_f_consignemnt_status_sms">
		<id name="consgStatusId" type="java.lang.Integer" column="Consignment_Status_Id">
			<generator class="native" />
		</id>

        <property name="contactNumber" column="Contact_number"/>
        <property name="consgNumber" column="Consignment_No"/>
        <property name="status" column="STATUS"/>
        <property name="smsId" column="SMS_ID"/>
        <property name="drsNumber" column="Drs_No"/>
        
	
	</class>
	<query name="getConsignmentStatusSmsDetails">
		<![CDATA[
			 from com.ff.domain.masters.ConsignmentStatusSmsDO cosignment 
			  where cosignment.consgNumber = :consgNo  
			  AND cosignment.contactNumber = :mobNumber
			  AND cosignment.status  =:consgStatus 
			 AND cosignment.drsNumber  =:drsNo]]>
	</query>
	
</hibernate-mapping>