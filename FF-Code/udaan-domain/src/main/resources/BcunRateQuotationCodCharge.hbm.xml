<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>


	<class name="com.ff.domain.ratemanagement.operations.ratequotation.BcunRateQuotationCODChargeDO" table="ff_f_rate_quotation_cod_charge">
		<id name="rateQuotationCODChargeId" type="java.lang.Integer" column="RATE_QUOTATION_COD_CHARGE_ID">
			<generator class="assigned" />
		</id>
		
		
		<property name="rateQuotationId" column="RATE_QUOTATION"/>
		<property name="considerHigherFixedOrPercent" >
        	<column name="CONSIDER_HIGHER_FIXED_OR_PERC"/>
		</property>
		
		<property name="considerFixed" >
        	<column name="CONSIDER_FIXED"/>
		</property>
				
		<property name="percentileValue" type="java.lang.Double"  >
        	<column name="PERCENTILE_VALUE"/>
		</property>
		
		<property name="fixedChargeValue">
        	<column name="FIXED_VALUE"/>
		</property>
		<property name="codChargeId" column="COD_CHARGE"/>
		<property name="dtToBranch" column="DT_TO_BRANCH"/>
    </class>

</hibernate-mapping>