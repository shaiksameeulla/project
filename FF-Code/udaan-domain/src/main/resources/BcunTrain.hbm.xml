<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.transport.BcunTrainDO" table="ff_d_train">
		<id name="trainId" type="java.lang.Integer" column="TRAIN_ID">
			<generator class="assigned" />
		</id>

        <property name="trainCode" column="TRAIN_CODE" type="java.lang.String"/>
        <property name="trainName" column="TRAIN_NAME" type="java.lang.String"/>
        <property name="trainNumber" column="TRAIN_NUMBER" type="java.lang.String"/>
			<property name="dtToBranch">
			<column name="DT_TO_BRANCH" />
		</property>	
	</class>
</hibernate-mapping>