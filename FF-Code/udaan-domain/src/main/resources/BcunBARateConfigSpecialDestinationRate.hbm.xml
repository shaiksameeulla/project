<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ff.domain.ratemanagement.operations.ba.BcunBARateConfigSpecialDestinationRateDO" table="ff_f_rate_ba_special_destination">
		<id name="specialDestinationId">
			<column name="RATE_BA_SPECIAL_DEST_ID" />
			<generator class="assigned" />
		</id>    
         <many-to-one name="city" class="com.ff.domain.geography.CityDO" lazy="false">
           <column name="CITY"/>
        </many-to-one>
         <property name="stateId">
            <column name="STATE_ID" />
        </property>
		<property name="rate">
			<column name="SLAB_RATE" />
		</property>
		<property name="servicedOn">
			<column name="SERVICED_ON" />
		</property>
		<property name="dtToBranch" column="DT_TO_BRANCH" />
		<property name="baRateProductHeaderId" column="BA_RATE_PRODUCT_HEADER" />
		<property name="baRateWeightSlabId" column="BA_WEIGHT_SLAB" />
	</class>
	
</hibernate-mapping>
	