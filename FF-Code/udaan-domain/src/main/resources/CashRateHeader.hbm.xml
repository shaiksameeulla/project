<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ff.domain.ratemanagement.operations.cash.CashRateHeaderDO" table="ff_f_rate_cash_rate_header">
		<id name="cashRateHeaderId">
			<column name="RATE_CASH_HEADER_ID" />
			<generator class="native" />
		</id>
		<property name="regionId">
            <column name="REGION" />
        </property>
		<property name="fromDate" type="java.util.Date">
            <column name="FROM_DATE" />
         </property>
		<property name="toDate" type="java.util.Date">
			<column name="TO_DATE" />
		</property>
		<property name="dtToBranch" column="DT_TO_BRANCH" />
		<property name="headerStatus">
			<column name="HEADER_STATUS" />
		</property>
	</class>

</hibernate-mapping>
	