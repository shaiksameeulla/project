<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ff.domain.serviceOffering.BcunRelationDO" table="ff_d_relation">
		<id name="relationId" type="java.lang.Integer">
			<column name="RELATION_ID" />
			<generator class="assigned" />
		</id>
		<property name="relationCode" column="RELATION_CODE" />
		<property name="relationDescription" column="RELATION_DESCRIPTION" />
		<property name="isActive" column="IS_ACTIVE" />
		<property name="dtToBranch" column="DT_TO_BRANCH" />
	</class>
	
</hibernate-mapping>