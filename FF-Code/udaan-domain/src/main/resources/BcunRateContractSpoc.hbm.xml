<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.ratemanagement.masters.BcunRateContractSpocDO" table="ff_d_rate_contract_spoc_contact_details">
		<id name="contractSpocId" type="java.lang.Integer">
			<column name="RATE_CONTRACT_CONTACT_DETAILS_ID" />
			<generator class="native" />
		</id>
		<property name="complaintType" column="COMPLAINT_TYPE"/>
		<property name="contactName" column="CONTACT_NAME"/>
		<property name="contactNo" column="CONTACT_NO"/>
		<property name="mobile" column="MOBILE"/>
		<property name="fax" column="FAX"/>
		<property name="email" column="EMAIL"/>
		<property name="contactType" column="CONTACT_TYPE"/>
		<property name="dtToBranch" column="DT_TO_BRANCH"/>
		<property name="rateContractDO" column="RATE_CONTRACT" type="java.lang.Integer" />
			
	</class>
	
</hibernate-mapping>