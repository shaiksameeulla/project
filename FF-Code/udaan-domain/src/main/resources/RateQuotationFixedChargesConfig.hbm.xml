<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>



	<class
		name="com.ff.domain.ratemanagement.operations.ratequotation.RateQuotationFixedChargesConfigDO"
		table="ff_f_rate_quotation_fixed_charges_config">
		<id name="quotaionFixedChargesConfigId" type="java.lang.Integer"
			column="ID">
			<generator class="native" />
		</id>

		<many-to-one name="rateQuotationDO" class="com.ff.domain.ratemanagement.operations.ratequotation.RateQuotationDO"  lazy="false" unique="true"  fetch="join">
			<column name="QUOTATION" />
		</many-to-one>
		


		<property name="insuredBy">
			<column name="INSURED_BY" />
		</property>

		<property name="octroiBourneBy">
			<column name="OCTROI_BOURNE_BY" />
		</property>
		<property name="dtToBranch" column="DT_TO_BRANCH"/>
		<property name="createdDate" column="CREATED_DATE" />
 		<property name="createdBy" column="CREATED_BY" />
        <property name="updatedBy" column="UPDATE_BY" />
		<property name="updatedDate" column="UPDATE_DATE"/>

	</class>
    <query name="getFixedChargesConfigForQuotation">
		<![CDATA[
			 SELECT  fixedChargesConfigDO.octroiBourneBy, fixedChargesConfigDO.insuredBy, riskSurchargeInsuredByDO.percentile
			 FROM	 com.ff.domain.ratemanagement.operations.ratequotation.RateQuotationFixedChargesConfigDO fixedChargesConfigDO, 
					 com.ff.domain.ratemanagement.masters.RiskSurchargeInsuredByDO  riskSurchargeInsuredByDO
			 WHERE 	 fixedChargesConfigDO.rateQuotationDO.rateQuotationId =:rateQuotationId
			 AND  	 fixedChargesConfigDO.insuredBy = riskSurchargeInsuredByDO.riskSurchargeInsuredByCode
		]]>
	</query>

  <query name="deleteFixedChargesConfig">
		<![CDATA[
			DELETE FROM com.ff.domain.ratemanagement.operations.ratequotation.RateQuotationFixedChargesConfigDO fixedChargesDO
			WHERE fixedChargesDO.rateQuotationDO.rateQuotationId =:rateQuotationId
		]]>
	</query>
    
</hibernate-mapping>