<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ff.domain.ratemanagement.operations.ba.BARateConfigAdditionalChargesDO" table="ff_f_rate_ba_additional_charges">
		<id name="additionalChrgId">
			<column name="RATE_BA_ADDL_CHARGE_ID" />
			<generator class="native" />
		</id>
		<property name="componentValue">
            <column name="COMPONENT_VALUE" />
        </property>
		<property name="componentCode">
            <column name="RATE_COMPONENT_CODE" />
         </property>
         <!-- <many-to-one name="baRateProductDO" class="com.ff.domain.ratemanagement.operations.ba.BaRateConfigProductHeaderDO" lazy="false">
           <column name="BA_RATE_PRODUCT_HEADER"/>
        </many-to-one>  -->
        <many-to-one name="baRateConfigHeaderDO"
			class="com.ff.domain.ratemanagement.operations.ba.BaRateConfigHeaderDO" lazy="false">
			<column name="BA_RATE_HEADER" />
		</many-to-one>
        <property name="dtToBranch" column="DT_TO_BRANCH" />
        <property name="priorityIndicator" column="PRIORITY_IND" />
        <property name="createdBy" column="CREATED_BY" update="false" />
		<property name="createdDate" column="CREATED_DATE" update="false" />
		<property name="updatedBy" column="UPDATE_BY" />
		<property name="updatedDate" column="UPDATE_DATE" />
	</class>
	<!--  <query name="deleteBAFixedCharges">
		<![CDATA[
			DELETE FROM com.ff.domain.ratemanagement.operations.ba.BARateConfigAdditionalChargesDO fixedChargesDO
			WHERE fixedChargesDO.baRateProductDO.baProductHeaderId =:baProductHeader
		]]>
	</query> -->
	
	<query name="getFixedChargesByHeaderIdAndPriorityIndicator">
		<![CDATA[
			FROM com.ff.domain.ratemanagement.operations.ba.BARateConfigAdditionalChargesDO fixedChargesDO
			WHERE fixedChargesDO.baRateConfigHeaderDO.headerId =:baRateConfigHeaderDO
			AND fixedChargesDO.priorityIndicator =:priorityIndicator
		]]>
	</query>
	
	
	
</hibernate-mapping>
	