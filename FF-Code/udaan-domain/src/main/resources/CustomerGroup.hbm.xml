<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>



	<class name="com.ff.domain.ratemanagement.masters.CustomerGroupDO"  table="ff_d_customer_group">
		<id name="customerGroupId" type="java.lang.Integer" column="CUSTOMER_GROUP_ID">
			<generator class="native" />
		</id>
		<property name="customerGroupCode" column="CUSTOMER_GROUP_CODE" type="java.lang.String" />
		<property name="customerGroupName" column="CUSTOMER_GROUP_NAME" type="java.lang.String" />
		<property name="dtToBranch" column="DT_TO_BRANCH"/>
   	</class>
    	
   	<query name="getCustomerGroupByCode">
		<![CDATA[ 
			FROM	com.ff.domain.ratemanagement.masters.CustomerGroupDO custGrpDO
			WHERE	custGrpDO.customerGroupCode=:customerGroupCode
		 ]]>
	 </query>

</hibernate-mapping>