<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.billing.SAPBillSalesOrderStagingDO"
		table="ff_f_sap_staging_bill_sales_order">
		<id name="id" type="java.lang.Long" column="ID">
			<generator class="increment" />
		</id>
		<property name="summaryId" column="SUMMARY_ID" />
		<property name="salesOrderNumber" column="SALES_ORDER_NUMBER" />
		<property name="billNumber" column="BILL_NUMBER" />
		<property name="billStatus" column="BILL_STATUS" />
		<property name="sapInbound" column="DT_SAP_INBOUND" />
		<property name="sapTimeStamp" column="SAP_TIMESTAMP" />
		<property name="grandTotal" column="GRAND_TOTAL" />
		<property name="billCreationDate" column="BILL_CREATION_DATE" />
	</class>

</hibernate-mapping>