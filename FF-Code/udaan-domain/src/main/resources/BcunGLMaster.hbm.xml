<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
	<class name="com.ff.domain.mec.BcunGLMasterDO" table="ff_d_gl_master">
		<id name="glId" type="java.lang.Integer">
			<column name="GL_ID" />
			<generator class="assigned" />
		</id>
		<property name="glCode" column="GL_CODE" />
		<property name="glDesc" column="GL_DESC" />
		<property name="nature" column="NATURE" />
		<property name="isEmpGL" column="IS_EMP_GL" />
		<property name="isCoLoaderGL" column="IS_CO_LOADER_GL" />
		<property name="isOctroiGL" column="IS_OCTROI_GL" />
		<property name="isBankGL" column="IS_BANK_GL" />
		<property name="paymentModeId" column="PAYMENT_ID" />
	</class>
	
</hibernate-mapping>