<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	
	<class name="com.ff.domain.ratemanagement.masters.RateComponentCalculatedDO" table="ff_d_rate_component_calculated">
		<id name="rateComponentCalculatedtId" type="java.lang.Integer" column="RATE_COMPONENT_CALCULATED_ID">
			<generator class="native" />
		</id>
		
			<many-to-one name="rateComponentCalculated" class="com.ff.domain.ratemanagement.masters.RateComponentDO"  lazy="false"  fetch="join">
		<column name="RATE_COMPONENT_CALCULATED" />
		</many-to-one>
		
			<many-to-one name="rateComponentCalculatedOn" class="com.ff.domain.ratemanagement.masters.RateComponentDO"  lazy="false"  fetch="join">
		<column name="RATE_COMPONENT_CALCULATED_ON" />
		</many-to-one>
		
		<property name="operationUsedInCalc" column="OPERATION_USED_IN_CALCULATION" type="java.lang.String" />
		
		<property name="evalOrder" column="ORDER_WITHIN"/>
      <property name="dtToBranch" column="DT_TO_BRANCH"/>
    </class>
 	<query name="getDependentRateComponentForCalculation">
		<![CDATA[
				FROM com.ff.domain.ratemanagement.masters.RateComponentCalculatedDO componentDependent
				ORDER BY componentDependent.rateComponentCalculated.calculationSequence, componentDependent.evalOrder
		]]>
	</query> 


</hibernate-mapping>