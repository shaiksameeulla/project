<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.ff.domain.ratemanagement.masters.VobSlabDO" table="ff_d_vob_slab">
		<id name="vobSlabId" type="java.lang.Integer" column="VOB_SLAB_ID">
			<generator class="native" />
		</id>
				
        <property name="lowerLimit" type="java.lang.Double" >
        <column name="LOWER_LIMIT" />
        </property>
        
        <property name="upperLimit" type="java.lang.Double" >
        <column name="UPPER_LIMIT" />
        </property>
        <property name="lowerLimitLabel"  column="LOWER_LIMIT_LABEL" />
        <property name="upperLimitLabel"  column="UPPPER_LIMIT_LABEL" />
        <property name="effectiveFrom"  column="EFFECTIVE_FROM" />
        <property name="effectiveTo"  column="EFFECTIVE_TO" />
        <property name="dtToBranch" column="DT_TO_BRANCH"/>
    </class>
    
  

</hibernate-mapping>