<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.ff.domain.ratemanagement.operations.ratecalculation.CashRateRTOChargesDO"
		table="ff_f_rate_cash_rto_charge">
		<id name="rateCashRTOChargesId" type="java.lang.Integer"
			column="RATE_CASH_RTO_CHARGES_ID">
			<generator class="native" />
		</id>
		<property name="rtoChargeApplicable" column="RTO_CHARGE_APPLICABLE" />
		<property name="sameAsSlabRate" column="SAME_AS_SLAB_RATE" />
		<property name="discountOnSlab" column="DISCOUNT_ON_SLAB" />
		<property name="rateComponentCode" column="RATE_COMPONENT_CODE" />
		<property name="cashProductHeaderId" column="CASH_RATE_HEADER" />
		<property name="dtToBranch" column="DT_TO_BRANCH" />
		<property name="priorityInd" column="PRIORITY_IND" />
	</class>
	
	<query name="getRTOChargesForCash">
		<![CDATA[
			 FROM  	com.ff.domain.ratemanagement.operations.ratecalculation.CashRateRTOChargesDO cashRateRTOChargesDO
			 WHERE 	cashRateRTOChargesDO.cashProductHeaderId =:header 
			 AND 	cashRateRTOChargesDO.priorityInd =:priorityInd
		]]>
	</query>
</hibernate-mapping>