<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.ff.domain.ratemanagement.masters.RateCustomerCategoryDO" table="ff_d_rate_customer_category">
		<id name="rateCustomerCategoryId" type="java.lang.Integer" column="RATE_CUSTOMER_CATEGORY_ID">
			<generator class="native" />
		</id>
				
        <property name="rateCustomerCategoryName" type="java.lang.String" >
        <column name="CUSTOMER_CATEGORY_NAME" />
        </property>
        
        <property name="rateCustomerCategoryCode" type="java.lang.String" >
        <column name="CUSTOMER_CATEGORY_CODE" />
       </property>
       <property name="dtToBranch" column="DT_TO_BRANCH"/>
    </class>
    
    <query name="getCustCategoryByCode">
	<![CDATA[ 
		FROM	com.ff.domain.ratemanagement.masters.RateCustomerCategoryDO rateCustCategoryDO
		WHERE	rateCustCategoryDO.rateCustomerCategoryCode=:rateCustomerCategoryCode
	 ]]>
	 </query>

</hibernate-mapping>