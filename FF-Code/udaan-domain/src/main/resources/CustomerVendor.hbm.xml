<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.booking.CustomerVendorDO" table="ff_f_customer_vendor">
		<id name="custVendorId" type="java.lang.Integer">
			<column name="CUST_VENDOR_ID" />
			<generator class="identity" />
		</id>
		<property name="custVendorCode"  type="java.lang.String">
		<column name="CUST_VENDOR_CODE" />
		</property>
		<property name="custVendorName"  type="java.lang.String">
		<column name="CUST_VENDOR_NAME" />
		</property>
		<property name="custVendorDesc"  type="java.lang.String">
		<column name="CUST_VENDOR_DESC" />
		</property>
		<property name="custVendorContactNum"  type="java.lang.String">
		<column name="CUST_VENDOR_CONTACT_NUM" />
		</property>
		<property name="status"  type="java.lang.String">
		<column name="STATUS" />
		</property>
		
		
	</class>
	
	
</hibernate-mapping>