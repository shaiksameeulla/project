<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.ff.domain.manifest.BcunBagRadioFrequencyDO" table="ff_d_bag_radio_freq_id">
		<id name="bagRfidId" type="java.lang.Integer">
			<column name="BAG_RFID_ID" />
			<generator class="assigned" />
		</id>
		
		<property name="rfidNo" type="java.lang.String">
			<column name="RFID_NO" />
		</property>
		
		<property name="dtToBranch">
			<column name="DT_TO_BRANCH" />
		</property>
		<property name="dtToCentral">
			<column name="DT_TO_CENTRAL" />
		</property>
         
	</class>
	
</hibernate-mapping>