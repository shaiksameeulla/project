<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.serviceOffering.ConsignmentTypeDO" table="ff_d_consignment_type">
		<id name="consignmentId" type="java.lang.Integer">
			<column name="CONSIGNMENT_TYPE_ID" />
			<generator class="assigned" />
		</id>
		<property name="consignmentCode" type="java.lang.String">
			<column name="CONSIGNMENT_CODE" />
		</property>
		<property name="consignmentName" type="java.lang.String">
			<column name="CONSIGNMENT_NAME" />
		</property>
		<property name="dtToBranch">
			<column name="DT_TO_BRANCH" />
		</property>	
		</class>
		
		 <query name="getConsignmentType">
	<![CDATA[ FROM com.ff.domain.serviceOffering.ConsignmentTypeDO )]]>
	</query>
	
</hibernate-mapping>