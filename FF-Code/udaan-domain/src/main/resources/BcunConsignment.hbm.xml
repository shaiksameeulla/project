	<query name="getConsgIdByConsgNo">
		<![CDATA[ SELECT consignment.consgId FROM 
				  com.ff.domain.consignment.BcunConsignmentDO consignment 
		          WHERE consignment.consgNo IN(:consgNumber)
		]]>
	
	</query>
	
	
	<query name="getConsgByConsgNo">
		<![CDATA[ 
			FROM com.ff.domain.consignment.BcunConsignmentDO consignment 
		    WHERE consignment.consgNo IN (:consgNo)
		]]>
	</query>
	
	<!-- Start of Changes by <48206> for opsman on 20/09/2013  -->
	<query name="getOpsmanInboundConsignment">
		<![CDATA[From com.ff.domain.consignment.BcunConsignmentDO consignment where consignment.dtToOpsman='N']]>
	</query>
	<!-- End of Changes by <48206> for opsman on 20/09/2013  -->
</hibernate-mapping>