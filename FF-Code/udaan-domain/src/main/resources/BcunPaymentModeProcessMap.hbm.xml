<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.serviceOffering.BcunPaymentModeProcessMapDO" table="ff_d_payment_mode_process_map">
		<id name="paymentModeProcessMapId" type="java.lang.Integer">
			<column name="PAYMENT_MODE_PROCESS_MAP_ID" />
			<generator class="assigned" />
		</id>
		<property name="status" >
			<column name="STATUS" />
		</property>
		
		<property name="paymentMode">
			<column name="PAYMENT_MODE_ID" />
		</property>
		
		<property name="process" >
			<column name="PROCESS_ID" />
		</property>
		
		<property name="dtToBranch">
			<column name="DT_TO_BRANCH" />
		</property>	
	</class>
</hibernate-mapping>