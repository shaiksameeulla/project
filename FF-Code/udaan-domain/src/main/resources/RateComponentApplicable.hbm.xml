<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.ff.domain.ratemanagement.masters.RateComponentApplicableDO"
	table="ff_d_rate_component_applicable">
	<id name="rateComponentApplicableId" type="java.lang.Integer"
		column="RATE_COMPONENT_APPLICABLE_ID">
		<generator class="native" />
	</id>

	<many-to-one name="rateComponent"
		class="com.ff.domain.ratemanagement.masters.RateComponentDO" lazy="false"
		fetch="join">
		<column name="RATE_COMPONENT" />
	</many-to-one>

	<many-to-one name="product" class="com.ff.domain.serviceOffering.ProductDO"
		lazy="false" fetch="join">
		<column name="PRODUCT" />
	</many-to-one>

	<many-to-one name="consignmentType"
		class="com.ff.domain.serviceOffering.ConsignmentTypeDO" lazy="false"
		fetch="join">
		<column name="CONSIGNMENT_TYPE" />
	</many-to-one>

	<property name="dtToBranch" column="DT_TO_BRANCH" />

</class>

</hibernate-mapping>