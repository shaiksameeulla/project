<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.billing.FinancialProductDO" table="ff_d_financial_product">
		<id name="financialProductId" column="FINANCIAL_PRODUCT_ID" type="java.lang.Integer" >
			<generator class="native" />
		</id>
		
		<property name="financialProductCode"  column="FINANCIAL_PRODUCT_CODE" type="java.lang.String" />
		<property name="financialProductName" column="FINANCIAL_PRODUCT_NAME" type="java.lang.String"/>
		<property  name="financialProductDesc" column="FINANCIAL_PRODUCT_DESCRIPTION"	type="java.lang.String"/>
		<property  name="status" column="STATUS" type="java.lang.String"/>
		<property name="dtToBranch">
			<column name="DT_TO_BRANCH" />
		</property> 
	</class>
	
	
</hibernate-mapping>