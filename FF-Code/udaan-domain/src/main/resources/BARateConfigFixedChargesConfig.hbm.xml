<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>



	<class
		name="com.ff.domain.ratemanagement.operations.ratecalculation.BARateConfigurationFixedChargesConfigDO"
		table="ff_f_rate_ba_fixed_charges_config">
		<id name="baFixedChargesConfigId" type="java.lang.Integer" column="BA_FIXED_CHARGES_CONFIG_ID">
			<generator class="native" />
		</id>

		<many-to-one name="baRateConfigHeaderDO"
			class="com.ff.domain.ratemanagement.operations.ba.BaRateConfigHeaderDO" lazy="false">
			<column name="BA_RATE_HEADER" />
		</many-to-one>

		<property name="insuredBy">
			<column name="INSURED_BY" />
		</property>

		<property name="octroiBourneBy">
			<column name="OCTROI_BOURNE_BY" />
		</property>
		<property name="dtToBranch" column="DT_TO_BRANCH" />
		<property name="priorityIndicator" column="PRIORITY_IND" />
		<property name="createdBy" column="CREATED_BY" update="false" />
		<property name="createdDate" column="CREATED_DATE" update="false" />
		<property name="updatedBy" column="UPDATE_BY" />
		<property name="updatedDate" column="UPDATE_DATE" />
	</class>
	<!-- <query name="deleteBAFixedChargesConfig">
		<![CDATA[
			DELETE FROM com.ff.domain.ratemanagement.operations.ratecalculation.BARateConfigurationFixedChargesConfigDO fixedChargeConfig
			WHERE fixedChargeConfig.baProductHeader.baProductHeaderId =:baProductHeader
		]]>
	</query> -->
	<query name="getBAFixedChargesConfigDetails">
		<![CDATA[
			FROM com.ff.domain.ratemanagement.operations.ratecalculation.BARateConfigurationFixedChargesConfigDO fixedChargeConfig
			WHERE fixedChargeConfig.baRateConfigHeaderDO.headerId =:baRateConfigHeaderDO
			AND fixedChargeConfig.priorityIndicator =:priorityIndicator
		]]>
	</query> 

</hibernate-mapping>