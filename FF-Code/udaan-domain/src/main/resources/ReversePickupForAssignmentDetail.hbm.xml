<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.pickup.ReversePickupForAssignmentDetailDO" table="ff_f_rev_pickup_order_request_detail">
		<id name="detailId" type="java.lang.Integer">
			<column name="REQUEST_DETAIL_ID" />
			<generator class="native" />
		</id>
		<property name="orderNumber" type="java.lang.String">
			<column name="ORDER_NO" />
		</property>
		<many-to-one name="pickupOrderHeader" class="com.ff.domain.pickup.ReversePickupOrderHeaderDO" fetch="join" lazy="false">
            <column name="REQUEST_HEADER_ID" not-null="true" />
        </many-to-one>
	</class>
</hibernate-mapping>