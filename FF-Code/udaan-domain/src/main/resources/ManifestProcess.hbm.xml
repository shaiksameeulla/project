		</property>
		
		<property name="multipleDestination" type="java.lang.String">
			<column name="MULTIPLE_DESTINATION" />
		</property>
		
		<property name="noOfElements" type="java.lang.Integer">
			<column name="NO_OF_ELEMENTS" />
		</property>
		
		<property name="containsOnlyCoMail" type="java.lang.String">
			<column name="CONTAINS_ONLY_COMAIL" />
		</property>
		
		<property name="manifestStatus" type="java.lang.String">
			<column name="MANIFEST_STATUS" />
		</property>
		
		<property name="bagRFID" type="java.lang.Integer">
			<column name="BAG_RFID" />
		</property>
		
		<property name="bplManifestType" type="java.lang.String">
			<column name="BPL_MANIFEST_TYPE" />
		</property>
		
		<property name="bagLockNo" type="java.lang.String">
			<column name="BAG_LOCK_NO" />
		</property>		
		
		<property name="loadLotId" type="java.lang.Integer">
			<column name="LOAD_LOT_ID" />
		</property>
		
		<property name="thirdPartyType" type="java.lang.String">
			<column name="THIRD_PARTY_TYPE" />
		</property>
		
		<property name="vendorId" type="java.lang.Integer">
			<column name="VENDOR_ID" />
		</property>
		
		<property name="dtToBranch">
			<column name="DT_TO_BRANCH" />
		</property>
		
		<property name="dtToCentral">
			<column name="DT_TO_CENTRAL" />
		</property>
		
		<property name="createdBy" column="CREATED_BY" update="false"/>
		<property name="updatedBy" column="UPDATE_BY" />
		<property name="createdDate"  column="CREATED_DATE"  update="false"/>
		<property  name="updatedDate" column="UPDATE_DATE" />
		
		<property  name="dtToOpsman" type="java.lang.String">
		<column name="DT_TO_OPSMAN"/>
		</property>
		
		<property  name="dtFromOpsman" 	type="java.lang.String">
		<column name="DT_FROM_OPSMAN"/>
		</property>
		
	</class>
