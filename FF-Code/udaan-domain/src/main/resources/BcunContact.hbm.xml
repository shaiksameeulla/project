<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.ratemanagement.masters.BcunContactDO"  table="ff_d_contact">
		<id name="contactId" type="java.lang.Integer" column="CONTACT_ID">
			<generator class="assigned" />
		</id>
		<property name="department" column="DEPARTMENT"/>
		<property name="name" column="NAME"/>
		<property name="title" column="TITLE"/>
		<property name="designation" column="DESIGNATION"/>
		<property name="email" column="EMAIL" />
		<property name="contactNo" column="CONTACT_NO" />
		<property name="extension" column="EXTN" />
		<property name="mobile" column="MOBILE"/>
		<property name="fax" column="FAX"/>
		 <property name="dtToBranch" column="DT_TO_BRANCH" />
    </class>
</hibernate-mapping>