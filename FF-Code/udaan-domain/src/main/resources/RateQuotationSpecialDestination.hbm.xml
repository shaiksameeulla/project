	<query name="deleteSpecialDestinationSlabRates">
	<![CDATA[
			DELETE FROM com.ff.domain.ratemanagement.operations.ratequotation.RateQuotationSpecialDestinationDO sd 
			WHERE sd.rateQuotationProductCategoryHeaderDO.rateQuotationProductCategoryHeaderId =:rateQuotationProductCategoryHeaderId
			
		]]>
	</query>
	
	<query name="deleteRateQuotationOriginSectorSplDestSlabRateRecords">
	<![CDATA[
			DELETE FROM com.ff.domain.ratemanagement.operations.ratequotation.RateQuotationSpecialDestinationDO sd 
			WHERE sd.rateQuotationProductCategoryHeaderDO.rateQuotationProductCategoryHeaderId =:rateQuotationProductCategoryHeaderId 
			AND sd.originSector =:originSector
		]]>
	</query>
</hibernate-mapping>