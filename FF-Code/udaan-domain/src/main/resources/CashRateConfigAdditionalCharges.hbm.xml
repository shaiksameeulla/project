<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ff.domain.ratemanagement.operations.cash.CashRateConfigAdditionalChargesDO" 
		table="ff_f_rate_cash_additional_charges">
		<id name="rateCashAdditionalChrgId">
			<column name="RATE_CASH_ADDITIONAL_CHRG_ID" />
			<generator class="native" />
		</id>
		<property name="componentValue">
			<column name="COMPONENT_VALUE" />
		</property>
		<property name="componentCode">
			<column name="COMPONENT_CODE" />
		</property>
		<property name="priorityInd">
			<column name="PRIORITY_IND" />
		</property>
        <many-to-one name="cashRateHeaderDO" class="com.ff.domain.ratemanagement.operations.cash.CashRateConfigHeaderDO" lazy="false"  fetch="join">
           	<column name="CASH_RATE_HEADER"/>
        </many-to-one>
        <property name="dtToBranch" column="DT_TO_BRANCH" />
	</class>
	<query name="getCashRateAdditionalChrgsDtls">
		<![CDATA[ 
			FROM com.ff.domain.ratemanagement.operations.cash.CashRateConfigAdditionalChargesDO additionalChrgs
			WHERE additionalChrgs.cashRateHeaderDO.cashRateHeaderId=:productMapId
		]]>
	</query>
	<query name="deleteCashRateAdditionalChrgDtls">
		<![CDATA[
			DELETE FROM com.ff.domain.ratemanagement.operations.cash.CashRateConfigAdditionalChargesDO additional
			WHERE additional.cashRateHeaderDO.cashRateHeaderId IN (:productMapIds)
		]]>
	</query>
</hibernate-mapping>
