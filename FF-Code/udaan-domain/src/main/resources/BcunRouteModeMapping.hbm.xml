<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.routeserviced.BcunRouteModeMappingDO" table="ff_d_route_mode_mapping">
		<id name="routeModeMappingId" type="java.lang.Integer" column="ROUTE_MODE_MAPPING_ID">
			<generator class="assigned" />
		</id>
		
		<property name="routeId" type="java.lang.Integer">
			<column name="ROUTE" />
		</property>
		
		<property name="transportModeId" type="java.lang.Integer">
			<column name="TRANSPORT_MODE" />
		</property>
		
		<property name="dtToBranch">
			<column name="DT_TO_BRANCH" />
		</property> 
	</class>
</hibernate-mapping>