<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.umc.CustomerUserDO" table="ff_d_customer_user">
		<id name="customerUserId" type="java.lang.Integer">
			<column name="CUSTOMER_USER_ID" />
			<generator class="identity" />
		</id>
		<property name="userId" type="java.lang.Integer">
			<column name="USER_ID" />
		</property>
				
		<many-to-one name="custDO" class="com.ff.domain.business.CustomerDO" lazy="false" fetch="join">
			<column name="CUSTOMER_ID" />
		</many-to-one>
		
		<property name="createdBy" type="java.lang.Integer">
            <column name="CREATED_BY" />
         </property>
         
          <property name="updatedBy" type="java.lang.Integer">
            <column name="UPDATE_BY" />
         </property>
         
          <property name="createdDate" type="java.util.Date">
            <column name="CREATION_DATE" />
         </property>
         
         <property name="updatedDate" type="java.util.Date">
            <column name="UPDATE_DATE" />
         </property>
         <property name="dtToBranch" column="DT_TO_BRANCH" />
	</class>
	
	<query name="getCustUserDObyUserId">
        <![CDATA[ from com.ff.domain.umc.CustomerUserDO customer where customer.userId=:userId ]]>
	</query>
	
	<query name="getCustUserDObyCustId">
        <![CDATA[ from com.ff.domain.umc.CustomerUserDO customer where customer.custDO.customerId=:customerId ]]>
	</query>
	
</hibernate-mapping>