<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.ff.domain.ratemanagement.masters.RateIndustryCategoryDO" table="ff_d_rate_industry_category">
		<id name="rateIndustryCategoryId" type="java.lang.Integer" column="RATE_INDUSTRY_CATEGORY_ID">
			<generator class="native" />
		</id>
				
        <property name="rateIndustryCategoryName" >
        <column name="INDUSTRY_CATEGORY_NAME" />
        </property>
        
        <property name="rateIndustryCategoryCode" >
        <column name="INDUSTRY_CATEGORY_CODE" />
        </property>
        
        <property name="rateCustomerCategoryId"  >
        <column name="RATE_CUSTOMER_CATEGORY" />
        </property>
        
        <property name="rateBenchMarkApplicable"  >
        <column name="RATE_BENCHMARK_APPLICABLE" />
        </property>
        
        <property name="rateQuotationApplicable"  >
        <column name="RATE_QUOTATION_APPLICABLE" />
        </property>
        <property name="dtToBranch" column="DT_TO_BRANCH"/>
    </class>

	<query name="getIndustryCategoryById">
	<![CDATA[
		FROM com.ff.domain.ratemanagement.masters.RateIndustryCategoryDO ind
			 WHERE ind.rateCustomerCategoryId IN(:rateIndustryCategoryIdList)
	
	]]>
	
	</query>
	
	<query name="getIndustryCategoryByCode">
	<![CDATA[
		FROM com.ff.domain.ratemanagement.masters.RateIndustryCategoryDO ind
			 WHERE ind.rateIndustryCategoryCode IN(:indCatCode)
	
	]]>
	
	</query>

</hibernate-mapping>