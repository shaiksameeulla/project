<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.complaints.ServiceRequestCustTypeDO" table="ff_d_service_request_cust_type">
		<id name="serviceRequestCustomerTypeId" type="java.lang.Integer">
			<column name="SERVICE_REQUEST_CUSTOMER_TYPE_ID" />
			<generator class="native" />
		</id>
		<property name="customerTypeCode" type="java.lang.String">
			<column name="CUSTOMER_TYPE_CODE" />
		</property>
		<property name="customerTypeName" type="java.lang.String">
			<column name="CUSTOMER_TYPE_NAME" />
		</property>
		<property name="customerTypeDescription" type="java.lang.String">
			<column name="CUSTOMER_TYPE_DESCRIPTION" />
		</property>
	</class>
	
	
	<query name="getComplaintsCustomerType">
		<![CDATA[ FROM com.ff.domain.complaints.ServiceRequestCustTypeDO as customerType
		          
		]]>
	</query>
		 	
</hibernate-mapping>