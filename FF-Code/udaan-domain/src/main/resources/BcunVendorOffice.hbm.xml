<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.business.BcunVendorOfficeDO" table="ff_d_vendor_office_map">
		<id name="vendorOfficeMappingId" column="VENDOR_OFFICE_MAP_ID" type="java.lang.Integer" >
			<generator class="assigned" />
		</id>
		<property name="vendorOfficeRegionId" column="VENDOR_OFFICE_REGION_ID" type="java.lang.Integer" />
		<!-- <property name="vendorId" column="VENDOR_ID" /> -->
		<many-to-one name="officeDO"
			class="com.ff.domain.organization.OfficeDO" lazy="false" fetch="join">
			<column name="OFFICE_ID" />
		</many-to-one>
		<property name="dtToBranch" column="DT_TO_BRANCH" />
	</class>

</hibernate-mapping>