	<query name="getTotalOldContractCount">
		<![CDATA[
          
          SELECT COUNT(*) FROM 
          com.ff.domain.billing.ReBillingConsignmentRateDO as rebillRate,
          com.ff.domain.billing.ReBillingConsignmentDO as rbc
           WHERE rbc.reBillingConsignmentId=rebillRate.reBillingConsignmentId And 
  	          rbc.reBillId =:RebillConsgId  And rebillRate.contractFor='O'
         
             ]]>
	</query>
   
   <query name="getTotalNewContractCount">
		<![CDATA[
          
           SELECT COUNT(*) FROM 
          com.ff.domain.billing.ReBillingConsignmentRateDO as rebillRate,
          com.ff.domain.billing.ReBillingConsignmentDO as rbc
           WHERE rbc.reBillingConsignmentId=rebillRate.reBillingConsignmentId And
  	          rbc.reBillId =:RebillConsgId  And rebillRate.contractFor='N'
         
             ]]>
	</query>
	
