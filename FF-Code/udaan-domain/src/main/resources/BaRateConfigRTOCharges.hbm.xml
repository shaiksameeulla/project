<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.ff.domain.ratemanagement.operations.ratecalculation.BARateConfigRTOChargesDO"
		table="ff_f_rate_ba_rto_charge">
		<id name="rateBARTOChargesId" type="java.lang.Integer" column="RATE_BA_RTO_CHARGES_ID">
			<generator class="native" />
		</id>
		<property name="rtoChargeApplicable" column="RTO_CHARGE_APPLICABLE" />
		<property name="sameAsSlabRate" column="SAME_AS_SLAB_RATE" />
		<property name="discountOnSlab" column="DISCOUNT_ON_SLAB" />
		<property name="rateComponentCode" column="RATE_COMPONENT_CODE" />

		<!-- <many-to-one name="baProductHeaderDO"
			class="com.ff.domain.ratemanagement.operations.ba.BaRateConfigProductHeaderDO"
			lazy="false">
			<column name="RATE_BA_PRODUCT_HEADER" />
		</many-to-one> -->
		<many-to-one name="baRateConfigHeaderDO"
			class="com.ff.domain.ratemanagement.operations.ba.BaRateConfigHeaderDO" lazy="false">
			<column name="BA_RATE_HEADER" />
		</many-to-one>
		<property name="dtToBranch" column="DT_TO_BRANCH" />
		<property name="priorityIndicator" column="PRIORITY_IND" />
		<property name="createdBy" column="CREATED_BY" update="false" />
		<property name="createdDate" column="CREATED_DATE" update="false" />
		<property name="updatedBy" column="UPDATE_BY" />
		<property name="updatedDate" column="UPDATE_DATE" />
	</class>

	<query name="getRTOChargesByHeader">
		<![CDATA[
			 FROM  com.ff.domain.ratemanagement.operations.ratecalculation.BARateConfigRTOChargesDO baRateRTOChargesDO
			 WHERE baRateRTOChargesDO.baRateConfigHeaderDO.headerId =:baRateConfigHeaderDO
			 AND baRateRTOChargesDO.priorityIndicator =:priorityIndicator
		]]>
	</query>
	<!-- <query name="deleteBaRTOChargesByProductHeader">
		<![CDATA[
			 DELETE FROM  com.ff.domain.ratemanagement.operations.ratecalculation.BARateConfigRTOChargesDO baRateRTOChargesDO
			 WHERE baRateRTOChargesDO.baProductHeaderDO.baProductHeaderId =:baProductHeader
		]]>
	</query> -->
	
</hibernate-mapping>