<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.complaints.ServiceRequestComplaintTypeDO" table="ff_d_service_request_complaint_type">
		<id name="serviceRequestComplaintTypeId" type="java.lang.Integer">
			<column name="SERVICE_REQUEST_COMPLAINT_TYPE_ID" />
			<generator class="native" />
		</id>
		<property name="complaintTypeCode" type="java.lang.String">
			<column name="COMPLAINT_TYPE_CODE" />
		</property>
		<property name="complaintTypeName" type="java.lang.String">
			<column name="COMPLAINT_TYPE_NAME" />
		</property>
		<property name="complaintTypeDescription" type="java.lang.String">
			<column name="COMPLAINT_TYPE_DESCRIPTION" />
		</property>
		<property name="isCriticalComplaint">
			<column name="IS_CRITICAL_COMPLAINT" />
		</property>
		
		
		
	</class>
		 	
</hibernate-mapping>