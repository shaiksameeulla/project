<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.serviceOffering.BcunProductDO" table="ff_d_product">
		<id name="productId" column="PRODUCT_ID" type="java.lang.Integer" >
			<generator class="assigned" />
		</id>
		<property name="productCode"  column="PRODUCT_CODE" type="java.lang.String" />
		<property name="productName" column="PRODUCT_NAME" type="java.lang.String"/>
		<property  name="ProductDesc" column="PRODUCT_DESC"	type="java.lang.String"/>
		<property  name="consgSeries" column="CONSG_SERIES" type="java.lang.String"/>
		<property  name="status" column="STATUS" type="java.lang.String"/>
		<property  name="dtToBranch" column="DT_TO_BRANCH"/>
		<property  name="productGroup" column="PROD_GROUP_ID"/>
		<property name="consolidationWindow" column="CONSOLIDATION_WINDOW"/>
	
	</class>
	
	
	
	
</hibernate-mapping>