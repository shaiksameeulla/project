<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ff.domain.ratemanagement.operations.cash.CashRateConfigHeaderProductDO" 
		table="ff_f_rate_cash_product_map">
		<id name="headerProductMapId">
			<column name="RATE_CASH_PRODUCT_MAP_ID" />
			<generator class="native" />
		</id>
        <many-to-one name="headerDO" class="com.ff.domain.ratemanagement.operations.cash.CashRateConfigHeaderDO" lazy="false"  fetch="join">
           <column name="RATE_CASH_HEADER" not-null="true"/>
        </many-to-one> 
        <property name="dtToBranch" column="DT_TO_BRANCH" />
        <property name="minChargeableWeight" column="MINIMUM_CHARGEABLE_WEIGHT" />
		<!-- <property name="productId">
            <column name="RATE_PRODUCT_CATEGORY" />
        </property> -->
        <many-to-one name="rateProductDO" class="com.ff.domain.ratemanagement.masters.RateProductCategoryDO" lazy="false"  fetch="join">
           <column name="RATE_PRODUCT_CATEGORY" not-null="true"/>
        </many-to-one> 
        
        <!-- one-to-many relation START -->
        <set name="cashRateSlabRateDOs" table="ff_f_rate_cash_slab_rate"
			lazy="false" cascade="all-delete-orphan" inverse="false" fetch="join">
			<key column="RATE_CASH_PRODUCT_MAP" />
			<one-to-many class="com.ff.domain.ratemanagement.operations.cash.CashRateSlabRateDO" />
		</set>
		<set name="cashRateSplDestDOs" table="ff_f_rate_cash_special_destination" 
			lazy="false" cascade="all-delete-orphan" inverse="false" fetch="join">
			<key column="RATE_CASH_PRODUCT_MAP" />
			<one-to-many class="com.ff.domain.ratemanagement.operations.cash.CashRateSpecialDestinationDO" />
		</set>
		<!-- one-to-many relation END -->
	</class>
</hibernate-mapping>
	