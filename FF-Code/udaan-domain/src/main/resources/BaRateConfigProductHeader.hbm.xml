<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class
		name="com.ff.domain.ratemanagement.operations.ba.BaRateConfigProductHeaderDO"
		table="ff_f_rate_ba_product_header">
		<id name="baProductHeaderId">
			<column name="RATE_BA_PRODUCT_HEADER_ID" />
			<generator class="native" />
		</id>
		<property name="rateProductCategory">
			<column name="RATE_PRODUCT_CATEGORY" />
		</property>
		<property name="isSave">
			<column name="ISSAVED" />
		</property>

		<many-to-one name="baRateHeaderDO"
			class="com.ff.domain.ratemanagement.operations.ba.BaRateConfigHeaderDO"
			lazy="false">
			<column name="RATE_BA_HEADER" />
		</many-to-one>

		<set name="baRateWeightSlabDOs" table="ff_f_rate_ba_weight_slab"
			cascade="all-delete-orphan" lazy="false" inverse="false">
			<key column="BA_RATE_PRODUCT_HEADER" />
			<one-to-many
				class="com.ff.domain.ratemanagement.operations.ba.BaRateWeightSlabDO" />
		</set>

		<property name="dtToBranch" column="DT_TO_BRANCH" />
		<property name="createdBy" column="CREATED_BY" update="false" />
		<property name="createdDate" column="CREATED_DATE" update="false" />
		<property name="updatedBy" column="UPDATE_BY" />
		<property name="updatedDate" column="UPDATE_DATE" />
	</class>

</hibernate-mapping>
	