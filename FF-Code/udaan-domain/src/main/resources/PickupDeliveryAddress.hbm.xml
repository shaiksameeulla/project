<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.pickup.AddressDO" table="ff_d_address">
		<id name="addressId" column="ADDRESS_ID" type="java.lang.Integer" >
			<generator class="native" />
		</id>
		
		<property  name="name" 	type="java.lang.String">
		<column name="NAME" />
         </property>
		
		<property  name="address1" type="java.lang.String">
		<column name="ADDRESS1" />
         </property>
         
         	<property  name="address2" type="java.lang.String">
		<column name="ADDRESS2" />
         </property>
         
         	<property  name="address3" type="java.lang.String">
		<column name="ADDRESS3" />
         </property>
	
		
		<property  name="phone" type="java.lang.String">
		<column name="PHONE" />
         </property>
         
		<property  name="mobile" type="java.lang.String">
		<column name="MOBILE" />
         </property>
         
         <property  name="email" type="java.lang.String">
		<column name="EMAIL" />
         </property>
         
          <property  name="status" type="java.lang.String">
		<column name="STATUS" />
         </property>
         
         
			<many-to-one name="cityDO" class="com.ff.domain.geography.CityDO"  lazy="false"  fetch="join">
		<column name="CITY" />
		</many-to-one>
		
		
			<many-to-one name="stateDO" class="com.ff.domain.geography.StateDO"  lazy="false"  fetch="join">
		<column name="STATE" />
		</many-to-one>
		
		
			<many-to-one name="pincodeDO" class="com.ff.domain.geography.PincodeDO"  lazy="false"  fetch="join">
		<column name="PINCODE" />
		</many-to-one>
        
        <property name="designation" type="java.lang.String">
			<column name="DESIGNATION" />
        </property>
        
        <property name="dtToBranch" column="DT_TO_BRANCH" />
         
		<property name="contactPerson" type="java.lang.String">
			<column name="CONTACT_PERSON" />
        </property>	
        
	</class>

</hibernate-mapping>