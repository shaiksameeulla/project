<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.complaints.ServiceRequestStatusDO" table="ff_d_service_request_status">
		<id name="serviceRequestStatusId" type="java.lang.Integer">
			<column name="SERVICE_REQUEST_STATUS_ID" />
			<generator class="native" />
		</id>
		<property name="statusCode" type="java.lang.String">
			<column name="STATUS_CODE" />
		</property>
		<property name="statusName">
			<column name="STATUS_NAME" />
		</property>	 	
		<property name="statusDescription">
			<column name="STATUS_DESCRIPTION" />
		</property>	 									
	</class>
</hibernate-mapping>