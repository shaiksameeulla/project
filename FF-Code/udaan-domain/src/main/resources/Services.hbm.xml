<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.complaints.ServicesDO" table="ff_d_services">
		<id name="serviceId" column="SERVICE_ID" type="java.lang.Integer" >
			<generator class="native" />
		</id>
		<property name="serviceCode" column="SERVICE_CODE" type="java.lang.String"/>
		<property name="serviceDescription" column="SERVICE_DESCRIPTION" type="java.lang.String"/>	
		<property name="dtToBranch" column="DT_TO_BRANCH" />
	</class>

</hibernate-mapping>