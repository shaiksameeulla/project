<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>



	<class name="com.ff.domain.ratemanagement.operations.ratecalculation.ConsignmentRateDO" table="ff_f_consignment_rate">
		<id name="consignmentRateId" type="java.lang.Integer" column="CONSIGNMENT_RATE_ID">
			<generator class="native" />
		</id>
		
		<property name="calculatedValue" column="CALCULATED_VALUE" type="java.lang.Double" />
		
		<property name="rtoCalculatedValue" column="RTO_CALCULATED_VALUE" type="java.lang.Double" />
		
		<many-to-one name="rateComponentDO" class="com.ff.domain.ratemanagement.masters.RateComponentDO"  lazy="false" fetch="join"  >
		<column name="RATE_COMPONENT" />
		</many-to-one>
		
		<many-to-one name="consignmentDO" class="com.ff.domain.consignment.ConsignmentDO"  lazy="false" fetch="join"  >
		<column name="CONSIGNMENT" />
		</many-to-one>
		
		<property  name="billed" 	type="java.lang.String">
		  <column name="BILLED"/>
		</property>
	
	</class>
	
	 <query name="getConsgRateId">
		<![CDATA[
			SELECT cnRate.consignmentRateId from com.ff.domain.ratemanagement.operations.ratecalculation.ConsignmentRateDO cnRate where cnRate.consignmentDO.consgNo = :consgNumber ]]>
	</query>

</hibernate-mapping>