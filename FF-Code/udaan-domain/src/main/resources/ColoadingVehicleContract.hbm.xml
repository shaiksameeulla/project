<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ff.domain.coloading.ColoadingVehicleContractDO" table="ff_d_coloading_vehicle_contract">
         <id name="id" type="java.lang.Integer">
            <column name="ID" />
            <generator class="native" />
        </id>
        <property name="vehicleNo" type="string">
            <column name="VEHICLE_NO" not-null="true"/>
        </property>
        <property name="vendorId" type="java.lang.Integer">
            <column name="VENDOR_ID" />
        </property>
        <property name="vehicleType" type="string">
            <column name="VEHICLE_TYPE" length="20" />
        </property>
        <property name="capacity" type="java.lang.Integer">
            <column name="CAPACITY" />
        </property>
        <property name="effectiveFrom" type="date">
            <column name="EFFECTIVE_FROM" length="10" not-null="true" />
        </property>
         <property name="effectiveTill" type="date">
            <column name="EFFECTIVE_TILL" length="10" />
        </property>
        <property name="rateType" type="string">
            <column name="RATE_TYPE" length="50" not-null="true" />
        </property>
        <property name="dutyHours" type="string">
            <column name="DUTY_HOURS" length="20"/>
        </property>
        <property name="average" type="java.lang.Double">
            <column name="AVERAGE" precision="10" scale="1" />
        </property>
        <property name="rent" type="double">
            <column name="RENT" precision="10" scale="1" />
        </property>
        <property name="freeKm" type="java.lang.Integer">
            <column name="FREE_KM" />
        </property>
        <property name="perKmRate" type="java.lang.Integer">
            <column name="PER_KM_RATE" />
        </property>
        <property name="perHourRate" type="java.lang.Integer">
            <column name="PER_HOUR_RATE" />
        </property>
        <property name="fuelType" type="string">
            <column name="FUEL_TYPE" length="10" not-null="true" />
        </property>
         <property name="noOfDays" type="java.lang.Integer">
            <column name="NO_OF_DAYS" />
        </property>
         <property name="others" type="java.lang.Integer">
           <column name="OTHERS" />
        </property>
         <property name="storeStatus" type="char">
            <column name="STORE_STATUS" length="1" not-null="true" />
        </property>
        <property name="createdBy" type="int">
            <column name="CREATED_BY" not-null="true" />
        </property>
        <property name="updatedBy" type="int">
            <column name="UPDATED_BY" not-null="true" />
        </property>
        <property name="createdDate" type="timestamp">
            <column name="CREATED_DATE" length="19" not-null="true" />
        </property>
        <property name="updatedDate" type="timestamp">
            <column name="UPDATED_DATE" length="19" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
