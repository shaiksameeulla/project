<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.common.BcunConfigurableParamsDO"
		table="ff_d_configurable_params">
		<id name="configurableParamsId" type="java.lang.Integer">
			<column name="PARAM_ID" />
			<generator class="assigned" />
		</id>

		<property name="paramValue" type="java.lang.String">
			<column name="PARAM_VALUE" length="100" />
		</property>
		<property name="paramName" type="java.lang.String">
			<column name="PARAM_NAME" length="100" />
		</property>
		<property name="dtToBranch">
			<column name="DT_TO_BRANCH" />
		</property>
	</class>

</hibernate-mapping>
