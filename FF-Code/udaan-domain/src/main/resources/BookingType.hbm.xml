<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.booking.BookingTypeDO" table="ff_d_booking_type">
		<id name="bookingTypeId" column="BOOKING_TYPE_ID" type="java.lang.Integer" >
			<generator class="assigned" />
		</id>
		<property  name="bookingType" column="BOOKING_TYPE"	type="java.lang.String"/>
		<property  name="bookingTypeDesc" column="BOOKING_TYPE_DESC"	type="java.lang.String"/>
		<property name="dtToBranch">
			<column name="DT_TO_BRANCH" />
		</property>	
	</class>

</hibernate-mapping>