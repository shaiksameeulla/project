<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ff.domain.serviceOffering.IdentityProofTypeDO" table="ff_d_identity_proof_type">
		<id name="identityProofTypeId" type="java.lang.Integer">
			<column name="IDENTITY_PROOF_DOC_ID" />
			<generator class="assigned" />
		</id>
		<property name="identityProofTypeCode" column="IDENTITY_PROOF_DOC_CODE" />
		<property name="identityProofTypeName" column="IDENTITY_PROOF_DOC_NAME" />
		<property name="issuingAuthority" column="ISSUING_AUTHORITY" />
		<property name="isActive" column="IS_ACTIVE" />
		<property name="dtToBranch" column="DT_TO_BRANCH" />
		
	</class>
	
</hibernate-mapping>