<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.business.VendorTypeDO" table="ff_d_vendor_type">
		<id name="vendorTypeId" column="VENDOR_TYPE_ID" type="java.lang.Integer" >
			<generator class="native" />
		</id>
		<property name="vendorTypeCode" column="VENDOR_TYPE_CODE" />
		<property name="vendorTypeDescription" column="VENDOR_TYPE_DESC" />
		<property name="accGroupSAP" column="ACCOUNT_GROUP_SAP" />
		<property name="dtToBranch" column="DT_TO_BRANCH"/>
	</class>
	
	<query name="getVendorTypeByAccGrp">
		<![CDATA[ 
			FROM	com.ff.domain.business.VendorTypeDO  vendorTypeDO
			WHERE 	vendorTypeDO.accGroupSAP=:accGroupSAP
		]]>
	</query>
	
</hibernate-mapping>