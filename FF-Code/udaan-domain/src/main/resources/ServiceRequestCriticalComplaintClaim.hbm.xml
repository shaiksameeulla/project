<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ff.domain.complaints.ServiceRequestCriticalComplaintClaimDO" table="ff_f_service_request_complaint_claim">
		<id name="serviceReqClaimId" type="java.lang.Integer" column="SERVICE_REQUEST_CLAIM_ID">
			<generator class="native" />
		</id>
		<property name="actualClaim" column="ACTUAL_CLAIM" />
		<property name="actualClaimAmt" column="ACTUAL_CLAIM_AMOUNT" />
		<property name="negotiableClaim" column="NEGOCIABLE_CLAIM" />
		<property name="negotiableClaimAmt" column="NEGOCIABLE_CLAIM_AMOUNT" />
		<property name="isCof" column="COF" />
		<property name="cofAmt" column="COF_AMOUNT" />
		<property name="settlement" column="SETTLEMENT" />
		<property name="isSettled" column="IS_SETTLED" />
		<property name="paperWork" column="PAPER_WORK" />
		<property name="accountability" column="ACCOUNTABILITY" />
		<property name="clientPolicy" column="CLIENT_POLICY" />
		<property name="missingCertificate" column="MISSING_CERTIFICATE" />
		<property name="remarks" column="REMARKS" />
		<property name="salesManagerFeedback" column="SALES_MANAGER_FEEDBACK" />
		<property name="salesManagerFeedbackDate" column="SALES_MANAGER_FEEDBACK_DATE" />
		<property name="csManagerFeedback" column="CS_MANAGER_FEEDBACK" />
		<property name="csManagerFeedbackDate" column="CS_MANAGER_FEEDBACK_DATE" />
		<property name="agmFeedback" column="AGM_FEEDBACK" />
		<property name="agmFeedbackDate" column="AGM_FEEDBACK_DATE" />
		<property name="vpFeedback" column="VP_FEEDBACK" />
		<property name="vpFeedBackDate" column="VP_FEEDBACK_DATE" />
		<property name="corporate" column="CORPORATE" />
		<property name="corporateDate" column="CORPORATE_DATE" />
		
		<property name="claimComplaintStatus" column="CLAIM_COMPLAINT_STATUS" />
		<!--  common attributes  START-->
		<property name="createdBy" column="CREATED_BY" />
		<property name="createdDate" column="CREATED_DATE" />
		<property name="updatedBy" column="UPDATE_BY" />
		<property name="updatedDate" column="UPDATE_DATE" />
		<!--  common attributes  END-->


	<many-to-one name="serviceRequestComplaintDO" class="com.ff.domain.complaints.ServiceRequestComplaintDO"  fetch="join" lazy="false">
		<column name="SERVICE_REQUEST_COMPLAINT_ID" />
	</many-to-one>
	

	</class>
	
	<query name="getCriticalClaimComplaintDtls">
		<![CDATA[
			FROM	com.ff.domain.complaints.ServiceRequestCriticalComplaintClaimDO criticalClaimComp 
		 	where	criticalClaimComp.serviceRequestComplaintDO.serviceRequestDO.serviceRequestNo=:serviceRequestNo
		]]>
	</query>
	
</hibernate-mapping>


