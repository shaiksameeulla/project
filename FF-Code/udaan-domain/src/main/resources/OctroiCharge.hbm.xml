<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	
	<class name="com.ff.domain.ratemanagement.masters.OctroiChargeDO" table="ff_d_octroi_charge">
		<id name="octroiChargeId" type="java.lang.Integer" column="OCTROI_CHARGE_ID">
			<generator class="native" />
		</id>
		
			<many-to-one name="rateComponent" class="com.ff.domain.ratemanagement.masters.RateComponentDO"  lazy="false" fetch="join">
		<column name="RATE_COMPONENT" />
		</many-to-one>
	
	
		<many-to-one name="rateCustomerCategory" class="com.ff.domain.ratemanagement.masters.RateCustomerCategoryDO"  lazy="false" fetch="join">
		<column name="RATE_CUSTOMER_CATEGORY" />
		</many-to-one>
		
		<property name="octroiBourneBy" column="OCTROI_BOURNE_BY" type="java.lang.String" />
		<property name="serviceCharge" column="SERVICE_CHARGE" type="java.lang.Double" />
		<property name="dtToBranch" column="DT_TO_BRANCH"/>

    </class>

</hibernate-mapping>