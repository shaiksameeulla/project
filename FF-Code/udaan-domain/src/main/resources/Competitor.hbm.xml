<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.leads.CompetitorDO" table="ff_d_competitor">
		<id name="competitorId" type="java.lang.Integer">
			<column name="COMPETITOR_ID" />
			<generator class="identity" />
		</id>
		<property name="competitorName" type="java.lang.String">
			<column name="COMPETITOR_NAME" />
		</property>
		<property name="dtToBranch">
			<column name="DT_TO_BRANCH" />
		</property>	 										
	</class>
		 	
</hibernate-mapping>