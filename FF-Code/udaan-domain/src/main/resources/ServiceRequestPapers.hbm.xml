<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.complaints.ServiceRequestPapersDO" table="ff_f_service_request_papers">
		<id name="serviceRequestPaperId" column="SERVICE_REQUEST_PAPER_ID" type="java.lang.Integer" >
			<generator class="native" />
		</id>
		<property name="serviceRequestId" column="SERVICE_REQUEST_ID"/>
		<property name="fileName" column="FILE_NAME"/>
		<property name="feedback" column="FEEDBACK"/>		
		<property name="clientMeet" column="CLIENT_MEET"/>
		<property name="createdDate" column="CREATED_DATE"/>
		<property name="createdBy" column="CREATED_BY"/>
		<property name="transferIcc" column="TRANSFER_ICC"/>
		<property name="file" column="FILE" />
		<property name="fileDescription" column="FILE_DESCRIPTION" />
	</class>

	<query name="getServiceRequestPapersByServiceRequestId">
		<![CDATA[ 	
			FROM com.ff.domain.complaints.ServiceRequestPapersDO serviceRequestPapersDO 
			WHERE serviceRequestPapersDO.serviceRequestId=:serviceRequestId
		]]>
	</query>	

	<query name="getPaperworkByServiceRequest">
		<![CDATA[
			FROM com.ff.domain.complaints.ServiceRequestPapersDO paperDO
			where paperDO.serviceRequestId=:serviceRequestId
		]]>
	</query>

	<query name="getPaperworkByPaperWorkId">
		<![CDATA[
			FROM com.ff.domain.complaints.ServiceRequestPapersDO paperWork
			where paperWork.serviceRequestPaperId=:paperWorkId
		]]>
	</query>

</hibernate-mapping>