<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.mec.BankDO" table="ff_d_bank_master">
		<id name="bankId" column="BANK_ID" type="java.lang.Integer" >
			<generator class="native" />
		</id>
		<property name="bankName" column="BANK_NAME" type="java.lang.String"/>
		<property name="bankGL" column="BANK_GL" type="java.lang.String"/>	
		
	</class>

<query name="getBankDtlsById">
	<![CDATA[ 
	FROM com.ff.domain.mec.BankDO bank WHERE bank.bankId=:bankId
	]]>
</query>
<query name="getAllBankDtls">
	<![CDATA[ FROM com.ff.domain.mec.BankDO ]]>
</query>

</hibernate-mapping>