<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.serviceability.PincodeBranchServiceabilityDO" table="ff_d_branch_pincode_serviceablity">
		<id name="pincodeBranchServiceId" column="PINCODE_SERVICES_BRANCH_ID" type="java.lang.Integer" >
			<generator class="native" />
		</id>
		
		<property  name="createdBy" column="CREATED_BY"	type="java.lang.Integer"/>
		<property  name="updateBy" column="UPDATE_BY"	type="java.lang.Integer"/>	
		<property  name="updateDate" column="UPDATE_DATE"	type="java.lang.String"/>
		<property  name="creationDate" column="CREATION_DATE"	type="java.lang.String"/>
		<property  name="dtToBranch" column="DT_TO_BRANCH"	type="java.lang.String"/>
		<property  name="status" column="STATUS"	type="java.lang.String"/>

		<many-to-one  name="pincode"  class="com.ff.domain.geography.PincodeDO" lazy="false" fetch="join">
			<column  name="PINCODE_ID" />
		</many-to-one>
		

		<many-to-one  name="office"  class="com.ff.domain.organization.OfficeDO" lazy="false" fetch="join">
			<column  name="OFFICE_ID" />
		</many-to-one>
	</class>
	
	<query name="getPincodeServicingBranch">
	<![CDATA[  
		select pinServ FROM com.ff.domain.serviceability.PincodeBranchServiceabilityDO pinServ 
		inner join pinServ.pincode as pincode
		where
		 pincode.pincode=:pincode 
		 and pincode.status='A'
		 and pinServ.status='A'
	]]>
	</query>
	
	<query name="getBranchServicingPincode">
	<![CDATA[  
		FROM com.ff.domain.serviceability.PincodeBranchServiceabilityDO pinServ where pinServ.office.officeCode=:officeCode
	]]>
	</query>
	
	<query name="getPincodeDetails">
		<![CDATA[  
			FROM com.ff.domain.serviceability.PincodeBranchServiceabilityDO  pincode where pincode.office.officeId=:officeId
		]]>
	</query>
	
</hibernate-mapping>	