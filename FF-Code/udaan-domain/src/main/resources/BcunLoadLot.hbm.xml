<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.ff.domain.manifest.BcunLoadLotDO" table="ff_d_load_lot">
		<id name="loadLotId" type="java.lang.Integer">
			<column name="LOAD_LOT_ID" />
			<generator class="assigned" />
		</id>
		
		<property name="loadNo" type="java.lang.Integer">
			<column name="LOAD_NO" />
		</property>
		
		<property name="loadDesc" type="java.lang.String">
			<column name="LOAD_DESC" />
		</property>
		
			<property name="dtToBranch">
			<column name="DT_TO_BRANCH" />
		</property>	
		
		<property name="dtToCentral">
			<column name="DT_TO_CENTRAL" />
		</property>
		
	</class>
         
     <!-- Starting Data sync querys -->
	<!-- <query name="getUnsyncInboundLoadLot">
		<![CDATA[From com.ff.domain.manifest.LoadLotDO loadLot where loadLot.dtToCentral='N' or loadLot.dtToCentral is null ]]>
	</query>
	
	<query name="getUpdateLoadLot"> 
	<![CDATA[ From com.ff.domain.manifest.LoadLotDO loadLot where  loadLot.loadNo IN (:loadNo) ]]>
	</query> -->
	 <!-- Ending Data sync querys -->

</hibernate-mapping>