<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.ratemanagement.masters.BcunContractPaymentBillingLocationDO" table="ff_d_contract_payment_billing_location">
		<id name="contractPaymentBillingLocationId" type="java.lang.Integer" column="CONTRACT_PAYMENT_BILLING_LOCATION_ID">
			<generator class="assigned" />
		</id>

        <property name="shippedToCode" column="SHIPPED_TO_CODE"/>        
        <property name="locationOperationType" column="LOCATION_OPERATION_TYPE"/>
		<property name="createdDate"  column="CREATED_DATE"/>
		<property  name="updatedDate" column="UPDATE_DATE"/>    
		<property name="createdBy" column="CREATED_BY"/>
		<property name="updatedBy" column="UPDATE_BY"/>
		<property name="status" column="status"/>
		<property name="rateContractId" column="RATE_CONTRACT"/>
		<property name="dtToBranch" column="DT_TO_BRANCH"/>
		
		<many-to-one name="pickupDeliveryLocation"
			class="com.ff.domain.pickup.BcunPickupDeliveryLocationDO" lazy="false" cascade="all" fetch="join">
			<column name="PICKUP_DELIVERY_LOCATION" />
		</many-to-one>
	</class>
		
</hibernate-mapping>