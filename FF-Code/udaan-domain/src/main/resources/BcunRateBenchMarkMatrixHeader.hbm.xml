<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.ff.domain.ratemanagement.operations.ratebenchmark.BcunRateBenchMarkMatrixHeaderDO" table="ff_d_rate_bench_mark_matrix_header">
		<id name="rateBenchMarkMatrixHeaderId" type="java.lang.Integer" column="RATE_BENCH_MARK_MATRIX_HEADER_ID">
			<generator class="assigned" />
		</id>
		
		<property name="vobSlab" column="VOB_SLAB" />
        <property name="rateMinChargeableWeight" column="RATE_MIN_CHARGEABLE_WEIGHT" />
        
        <many-to-one name="rateBenchMarkProductDO" class="com.ff.domain.ratemanagement.operations.ratebenchmark.BcunRateBenchMarkProductDO" cascade="all" lazy="false"  fetch="join">
        <column name="RATE_BENCH_MARK_PRODUCT" ></column>
        </many-to-one>
        
        <set name="rateBenchMarkMatrixDO" table = "ff_d_rate_bench_mark_matrix" cascade="all" inverse="true" lazy="false" fetch="join">
		<key column="RATE_BENCH_MARK_MATRIX_HEADER" />
		<one-to-many class="com.ff.domain.ratemanagement.operations.ratebenchmark.BcunRateBenchMarkMatrixDO" />
		</set>
		
		<property name="dtToBranch" column="DT_TO_BRANCH"/>
    </class>

</hibernate-mapping>