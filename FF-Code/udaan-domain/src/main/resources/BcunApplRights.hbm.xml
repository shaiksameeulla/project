<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ff.domain.umc.bcun.BcunAppsRightsDO" table="ff_d_app_rights">
		<id name="rightsId" type="java.lang.Integer">
			<column name="RIGHTS_ID" />
			<generator class="assigned" />
		</id>
		
		<property name="roleId" column="ROLE_ID"/>
		<property name="screenId" column="SCREEN_ID"/>
		<property name="createdBy" column="CREATED_BY" />
		<property name="transactionCreateDate" column="CREATION_DATE"/>
		<property name="updatedBy" column="UPDATE_BY"/>
		<property name="transactionModifiedDate" column="UPDATE_DATE"/>
		
		<property name="dtToBranch">
			<column name="DT_TO_BRANCH" />
		</property>
		
		<property name="transactionStatus">
			<column name="STATUS" />
		</property>
	</class>
	<query name="getUnsyncOutboundApplsRights">
		<![CDATA[
		from com.ff.domain.umc.bcun.BcunAppsRightsDO applRights where
						  (applRights.dtToBranch='N')
						 
						 ]]>
	</query>
	
</hibernate-mapping>
	