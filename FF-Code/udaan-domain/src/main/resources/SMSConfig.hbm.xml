<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ff.domain.serviceOffering.SMSConfigDO" table="ff_d_sms_config">
		<id name="smsId" type="java.lang.Integer">
			<column name="SMS_ID" />
			<generator class="assigned"/>
		</id>
		<property name="shiftStartTime" type="java.lang.String">
			<column name="SHIFT_START_TIME" />
		</property>
				
		<property name="shiftEndTime" type="java.lang.String">
			<column name="SHIFT_END_TIME" />
		</property>
		
		<property name="moduleName" type="java.lang.String">
			<column name="MODULE_NAME" />
		</property>
		
		<property name="mobileNumber" type="java.lang.Integer">
			<column name="MOBILE_NUMBER" />
		</property>
		
		<property name="employeeName" type="java.lang.String">
			<column name="EMPLOYEE_NAME" />
		</property>
		<property name="city" type="java.lang.String">
			<column name="CITY" />
		</property>
		
		<property name="dtToBranch">
			<column name="DT_TO_BRANCH" />
		</property>										
	</class>
	
</hibernate-mapping>