<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
   
	<class name="com.ff.domain.ratemanagement.masters.BcunRateTaxComponentDO" table="ff_d_rate_tax_component">
		<id name="taxComponentId" type="java.lang.Integer" column="TAX_COMPONENT_ID">
			<generator class="assigned" />
		</id>
		<property name="rateComponentCode" column="RATE_COMPONENT_CODE"/>
		<property name="state" column="STATE"/>
		<property name="taxPercentile" column="TAX_PERCENTILE" />
		<property name="activeStatus" column="ACTIVE_STATUS" />
		<property name="effectiveFrom" column="EFFECTIVE_FROM"  />
		<property name="effectiveTo" column="EFFECTIVE_TO"  />
		<property name="taxGroup" column="TAX_GROUP"/>
		<property name="dtToBranch" column="DT_TO_BRANCH" />
    </class>
   
</hibernate-mapping>