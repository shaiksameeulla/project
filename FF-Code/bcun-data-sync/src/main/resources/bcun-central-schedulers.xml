	 <import resource="classpath:bcun-outbound-central-category-d-scheduler.xml"/>
	 
	<!-- This Category E scheduler is used to prepare other transaction blob data at central server for Category E office -->
	 <import resource="classpath:bcun-outbound-central-category-e-scheduler.xml"/>
	 
	
	<!-- This scheduler is used to prepare consignment blob data at central server -->
	 <import resource="classpath:bcun-outbound-central-consignment-scheduler.xml"/> 
	 
	<!-- This scheduler is used to prepare manifest blob data at central server -->
	 <import resource="classpath:bcun-outbound-central-manifest-scheduler.xml"/> 
		
