	 <import resource="classpath:bcun-outbound-central-manifest-category-a-scheduler.xml"/>
	 
	<!-- This scheduler is used to prepare manifest blob data at central server for Category B office -->
	 <import resource="classpath:bcun-outbound-central-manifest-category-b-scheduler.xml"/>
	 
	<!-- This scheduler is used to prepare manifest blob data at central server for Category C office -->
	 <import resource="classpath:bcun-outbound-central-manifest-category-c-scheduler.xml"/>
	 
	<!-- This scheduler is used to prepare manifest blob data at central server for Category D office -->
	 <import resource="classpath:bcun-outbound-central-manifest-category-d-scheduler.xml"/>
	 
	<!-- This scheduler is used to prepare manifest blob data at central server for Category E office -->
	 <import resource="classpath:bcun-outbound-central-manifest-category-e-scheduler.xml"/>
	
	<!-- This scheduler is used to prepare pickup blob data at central server -->
	 <import resource="classpath:bcun-outbound-central-pickup-scheduler.xml"/>
	
	<!-- This scheduler is used to prepare stock blob data at central server -->
	 <import resource="classpath:bcun-outbound-central-stock-scheduler.xml"/>
	
	<!-- This scheduler is used to prepare umc blob data at central server -->
	 <import resource="classpath:bcun-outbound-central-umc-scheduler.xml"/>
	 
	 <!-- This scheduler is used to prepare Drs blob data at central server -->
<!-- 	<import resource="classpath:bcun-outbound-central-drs-scheduler.xml" /> -->
	
	<!-- This scheduler is used to prepare transactional master data blob at central server -->
	 <import resource="classpath:bcun-outbound-central-transactional-master-scheduler.xml"/>
	 
	<!-- This scheduler is used to prepare and upload(in queue/DB) all inbound data -->
	<import resource="classpath:bcun-inbound-central-scheduler.xml"/>
	
	<!-- This scheduler is used to prepare create Blob data at central server for Bcun Reconciliation -->
	<import resource="classpath:bcun-outbound-central-data-reconcillation-scheduler.xml"/> 
	<!-- This scheduler is used to download the file and update at central side-->
	<import resource="classpath:bcun-inbound-central-reconciliation-scheduler.xml"/>  
	
	<!-- This scheduler is used for cleaning activities at central server -->
	<import resource="classpath:bcun-cleanup-schedulers.xml" />
		 
