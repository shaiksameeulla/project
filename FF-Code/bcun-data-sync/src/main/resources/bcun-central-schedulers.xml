	<!-- This scheduler is used to prepare other transaction data blob data at central server -->
	 <import resource="classpath:bcun-outbound-central-scheduler.xml"/>
	 
	<!-- This Category A scheduler is used to prepare other transaction blob data at central server for Category A office-->
	 <import resource="classpath:bcun-outbound-central-category-a-scheduler.xml"/>
	 
	<!-- This Category B scheduler is used to prepare other transaction blob data at central server for Category B office -->
	 <import resource="classpath:bcun-outbound-central-category-b-scheduler.xml"/>
	 
