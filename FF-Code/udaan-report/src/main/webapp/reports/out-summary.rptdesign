            <method name="beforeOpen"><![CDATA[var sortingOrder = "";
var fromDate = "";
var toDate = "";
var reportName = "";
var reportType = "";
var summaryOption = "";
var products = "";
productCount = "";
var sorting = "";
var regions = "";
regionCount = "";
var customers = "";
customerCount = "";

var selectClause = " SELECT ";
var fromClause = " FROM ";
fromClause = fromClause + "	  ff_fct_book_to_bill ffbtb ";
//fromClause = fromClause + "   JOIN ff_f_consignment ffc on ffc.CONSG_ID = ffbtb.CONSG_ID ";
//fromClause = fromClause + "   JOIN ff_f_booking ffb ON ffb.CONSG_NUMBER = ffbtb.CONSG_NO ";
fromClause = fromClause + "   JOIN ff_d_customer fdc ON ffbtb.CUSTOMER_C = fdc.CUSTOMER_ID ";
fromClause = fromClause + "   JOIN ff_d_office fdo ON fdo.OFFICE_ID = fdc.SALES_OFFICE ";
//fromClause = fromClause + "   JOIN ff_d_pincode fdpin_dest ON fdpin_dest.PINCODE_ID = ffbtb.DEST_PINCODE ";
//fromClause = fromClause + "   JOIN ff_d_city fdc_dest ON fdc_dest.CITY_ID = fdpin_dest.CITY_ID ";
fromClause = fromClause + "   JOIN ff_d_product fdp ON ffbtb.PRODUCT = fdp.PRODUCT_ID ";
fromClause = fromClause + "   LEFT JOIN ff_mv_latest_delivery ffdd ";
fromClause = fromClause + "     ON ffbtb.CONSG_ID = ffdd.CONSG_ID ";
fromClause = fromClause + "   LEFT JOIN ff_f_delivery_dtls ffdd1 ";
fromClause = fromClause + "     ON ffdd1.DELIVERY_DETAIL_ID = ffdd.DELIVERY_DETAIL_ID ";
//fromClause = fromClause + "   LEFT JOIN ff_f_delivery ffd ON ffd.DELIVERY_ID = ffdd.DELIVERY_ID ";
fromClause = fromClause + "   LEFT JOIN ff_f_collection_entries ffce ON ffce.CONSIGNMENT_ID = ffbtb.CONSG_ID ";
fromClause = fromClause + "   LEFT JOIN ff_d_reason fdr ON fdr.REASON_ID = ffce.REASON_ID ";
var whereClause = " WHERE ";
var groupByClause = " GROUP BY ";
var orderByClause = " ORDER BY ";

/*
 * Build Select Clause
 */

// get summary option
if ( params["SummaryOption"] != null &amp;&amp; params["SummaryOption"].length > 0 ){
	summaryOption = params["SummaryOption"];
}
if ( BirtComp.equalTo(summaryOption,'DT') ){
	selectClause = selectClause + " date_format(ffbtb.EVENT_DATE, '%d/%m/%Y') as EVENT_DATE, ";
} if ( BirtComp.equalTo(summaryOption,'MTH') ){
	selectClause = selectClause + " date_format(ffbtb.EVENT_DATE, '%b, %Y') as EVENT_DATE, ";
}
selectClause = selectClause + " sum( CASE ";
selectClause = selectClause + " 	 WHEN fdp.CONSG_SERIES = 'L' THEN ffbtb.COD_AMT ";
selectClause = selectClause + " 	 WHEN fdp.CONSG_SERIES = 'D' THEN ffbtb.LC_AMT ";
selectClause = selectClause + " 	 ELSE 0 ";
selectClause = selectClause + " 	 END) ";
selectClause = selectClause + " AS AMOUNT, ";
selectClause = selectClause + " count(ffbtb.CONSG_NO) as COUNT ";

/*
 * Build Where Clause
 */

// get sorting order
if ( params["SortingOrder"] != null &amp;&amp; params["SortingOrder"].length > 0 ){
	SortingOrder = params["SortingOrder"];
}
	whereClause = whereClause + "  ffbtb.rate_calculated_for = 'B' ";
// get from date and to date
if(params["FromDate"] != null){
	fromDate = params["FromDate"];
	if( BirtComp.equalTo(SortingOrder,"BKDT") ){
		whereClause = whereClause + " AND date(ffbtb.BOOKING_DATE) BETWEEN str_to_date('" + fromDate + "', '%Y-%m-%d')";
	} else if( BirtComp.equalTo(SortingOrder,"EVTDT") ){
		whereClause = whereClause + " date(ffbtb.EVENT_DATE) BETWEEN str_to_date('" + fromDate + "', '%Y-%m-%d')";
	}
}
if(params["ToDate"] != null){
	toDate = params["ToDate"];
	whereClause = whereClause + " AND str_to_date('" + toDate + "', '%Y-%m-%d')";
}

// get customers
if ( params["Customers"] != null &amp;&amp; params["Customers"].length > 0 ){
	customers = params["Customers"].split(",");
}
customerCount = customers.length;
if ( customerCount > 0 ){
	whereClause = whereClause + " AND fdc.CUSTOMER_ID in ( 0, "

	for ( i=0; i < customerCount; i++ ){
	  	if( i == 0 ){
			whereClause = whereClause + "" + customers[i];
		} else if ( i <= customerCount-1 ){
			whereClause = whereClause + ", " + customers[i];
	  	} else {
			whereClause = whereClause + ", " + customers[i];
		}
	}
	
  	whereClause = whereClause + " ) ";
  	
}

// get regions
if ( params["Region"] != null &amp;&amp; params["Region"].length > 0 ){
	regions = params["Region"].split(",");
}
regionCount = regions.length;
if ( regionCount > 0 ){
	whereClause = whereClause + " AND fdo.MAPPED_TO_REGION IN ( "

	for ( i=0; i < regionCount; i++ ){
	  	if( i == 0 ){
			whereClause = whereClause + regions[i];
		} else if ( i <= regionCount-1 ){
			whereClause = whereClause + ", " + regions[i];
	  	} else {
			whereClause = whereClause + ", " + regions[i];
		}
	}
	
  	whereClause = whereClause + " ) ";
  	
}

// get products
if ( params["Products"] != null &amp;&amp; params["Products"].length > 0 ){
	products = params["Products"].split(",");
}
productCount = products.length;
if ( productCount > 0 ){
	whereClause = whereClause + " AND fdp.CONSG_SERIES IN ( "

	for ( i=0; i < productCount; i++ ){
	  	if( i == 0 ){
			whereClause = whereClause + "'" + products[i] + "'";
		} else if ( i <= productCount-1 ){
			whereClause = whereClause + ", '" + products[i] + "'";
	  	} else {
			whereClause = whereClause + ", '" + products[i];
		}
	}
	
  	whereClause = whereClause + " ) ";
  	
}

// get report type
if ( params["Type"] != null &amp;&amp; params["Type"].length > 0 ){
	reportType = params["Type"];
}
if (BirtComp.equalTo(reportType,'ALL')) {
	whereClause = whereClause + "";
} else if (BirtComp.equalTo(reportType,'BLNK')) {
	whereClause = whereClause + " AND ";
	whereClause = whereClause + " (ffdd.DELIVERY_STATUS != 'D' OR ";
	whereClause = whereClause + " ffbtb.CONSG_STATUS != 'R') ";
} else if (BirtComp.equalTo(reportType,'RTO')) {
	whereClause = whereClause + " AND ";
	whereClause = whereClause + " ffbtb.CONSG_STATUS = 'R' ";
} else if (BirtComp.equalTo(reportType,'PL')) {
	whereClause = whereClause + " AND ";
	whereClause = whereClause + " ffdd.DELIVERY_STATUS = 'D' AND ";
	whereClause = whereClause + " fdr.REASON_CODE = '24' ";
} else if (BirtComp.equalTo(reportType,'CHQ')) {
	whereClause = whereClause + " AND ";
	whereClause = whereClause + " ffdd.DELIVERY_STATUS = 'D' AND ";
	whereClause = whereClause + " fdr.REASON_CODE != '24' AND ";
	whereClause = whereClause + " ffdd1.MODE_OF_PAYMENT = 'CHEQUE' "; 
} else if (BirtComp.equalTo(reportType,'CSH')) {
	whereClause = whereClause + " AND ";
	whereClause = whereClause + " ffdd.DELIVERY_STATUS = 'D' AND ";
	whereClause = whereClause + " fdr.REASON_CODE != '24' AND ";
	whereClause = whereClause + " ffdd1.MODE_OF_PAYMENT = 'CASH' ";
}

/*
 * Build Group By Clause
 */

if ( BirtComp.equalTo(summaryOption,'DT') ){
	groupByClause = groupByClause + " date_format(ffbtb.EVENT_DATE, '%d/%m/%Y') ";
} if ( BirtComp.equalTo(summaryOption,'MTH') ){
	groupByClause = groupByClause + " date_format(ffbtb.EVENT_DATE, '%b, %Y') ";
}

/*
 * Build Order By Clause
 */

if ( BirtComp.equalTo(summaryOption,'DT') ){
	orderByClause = orderByClause + " ffbtb.EVENT_DATE ";
} if ( BirtComp.equalTo(summaryOption,'MTH') ){
	orderByClause = orderByClause + " ffbtb.EVENT_DATE ";
}


this.queryText = selectClause + fromClause + whereClause + groupByClause + orderByClause;
//logToDebugWindow("dsOutSummary Generated Query is - ");
//logToDebugWindow(this.queryText);

// get sorting order
/*if ( params["SortingOrder"] != null &amp;&amp; params["SortingOrder"].length > 0 ){
	SortingOrder = params["SortingOrder"];
}*/
// get report name
/*if ( params["Report"] != null &amp;&amp; params["Report"].length > 0 ){
	reportName = params["Report"];
}*/
// get sorting
/*if ( params["Sorting"] != null &amp;&amp; params["Sorting"].length > 0 ){
	sorting = params["Sorting"];
}*/]]></method>
