            <xml-property name="queryText"><![CDATA[SELECT
  fdc.BUSINESS_NAME
 ,sum(
    CASE
      WHEN fdp.CONSG_SERIES = 'L' THEN ffbtb.COD_AMT
      WHEN fdp.CONSG_SERIES = 'D' THEN ffbtb.LC_AMT
      ELSE 0
    END)
    AS AMOUNT
FROM
  ff_fct_book_to_bill ffbtb
  JOIN ff_d_customer fdc ON ffbtb.CUSTOMER_C = fdc.CUSTOMER_ID
  JOIN ff_d_office fdo ON fdo.OFFICE_ID = fdc.SALES_OFFICE
  JOIN ff_d_product fdp ON ffbtb.PRODUCT = fdp.PRODUCT_ID
  LEFT JOIN ff_mv_latest_delivery ffdd ON ffbtb.CONSG_ID = ffdd.CONSG_ID
  LEFT JOIN ff_f_delivery_dtls ffdd1
    ON ffdd1.DELIVERY_DETAIL_ID = ffdd.DELIVERY_DETAIL_ID
  LEFT JOIN ff_f_delivery ffd ON ffd.DELIVERY_ID = ffdd.DELIVERY_ID
  LEFT JOIN ff_f_collection_entries ffce
    ON ffce.CONSIGNMENT_ID = ffbtb.CONSG_ID
  LEFT JOIN ff_d_reason fdr ON fdr.REASON_ID = ffce.REASON_ID]]></xml-property>
