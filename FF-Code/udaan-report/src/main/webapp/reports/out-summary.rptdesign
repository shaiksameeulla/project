            <xml-property name="queryText"><![CDATA[SELECT
  /* for Summary Option 'Datewise' */
--   ffc.EVENT_DATE,
  /* for Summary Option 'Monthwise' */
  date_format(ffc.EVENT_DATE, '%b, %Y') as EVENT_DATE,
  sum( CASE WHEN fdp.CONSG_SERIES = 'L' THEN ffc.COD_AMT WHEN fdp.CONSG_SERIES = 'D' THEN ffc.LC_AMT ELSE 0 END)
    AS AMOUNT,
    count(ffc.CONSG_NO) as COUNT
FROM
  ff_f_consignment ffc
  JOIN ff_f_booking ffb ON ffb.CONSG_NUMBER = ffc.CONSG_NO
  JOIN ff_d_customer fdc ON ffb.CUSTOMER = fdc.CUSTOMER_ID
  JOIN ff_d_office fdo ON fdo.OFFICE_ID = fdc.SALES_OFFICE
  JOIN ff_d_product fdp ON ffc.PRODUCT = fdp.PRODUCT_ID
  LEFT JOIN ff_f_delivery_dtls ffdd
    ON (ffc.CONSG_ID = ffdd.CONSIGNMENT_ID AND
        ffdd.DELIVERY_STATUS = 'D')
  LEFT JOIN ff_f_collection_entries ffce ON ffce.CONSIGNMENT_ID = ffc.CONSG_ID
  LEFT JOIN ff_d_reason fdr ON fdr.REASON_ID = ffce.REASON_ID
WHERE
  ffb.BOOKING_DATE BETWEEN str_to_date(
                             '01/03/2014',
                             '%d/%m/%Y')
                       AND str_to_date(
                             '12/03/2014',
                             '%d/%m/%Y') AND
  fdc.CUSTOMER_ID IN (3336) AND
  fdo.MAPPED_TO_REGION IN (2) AND
  fdp.CONSG_SERIES IN ('L',
                       'D')
--   /*
--    * criteria for Type 
--    */
--   /* no condition for Type 'All', every consignment between date range sould be considered */
--   /* condition for Type 'Blank' */
--   AND
--   ffc.CONSG_STATUS NOT IN ('D',
--                            'R') 
--   /* condition for Type 'RTO' */
--   AND
--   ffc.CONSG_STATUS = 'R' 
--   /* condition for Type 'Party Letter' */
--   AND
--   ffc.CONSG_STATUS = 'D' AND
--   fdr.REASON_CODE = '24' 
--   /* condition for Type 'Cheque' */
--   AND
--   ffc.CONSG_STATUS = 'D' AND
--   fdr.REASON_CODE != '24' AND
--   ffdd.MODE_OF_PAYMENT = 'CHEQUE' 
--   /* condition for Type 'Cash' */
--   AND
--   ffc.CONSG_STATUS = 'D' AND
--   fdr.REASON_CODE != '24' AND
--   ffdd.MODE_OF_PAYMENT = 'CASH'
GROUP BY
  /* for Summary Option 'Datewise' */
--  ffc.EVENT_DATE
--   /* for Summary Option 'Monthwise' */
   date_format(ffc.EVENT_DATE, '%b, %Y')
ORDER BY
  /* for Summary Option 'Datewise' */
--  ffc.EVENT_DATE
--   /* for Summary Option 'Monthwise' */
   date_format(ffc.EVENT_DATE, '%b, %Y')
;]]></xml-property>
