<?xml version="1.0" encoding="ISO-8859-1" ?>

<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
          "http://struts.apache.org/dtds/struts-config_1_3.dtd">


<struts-config>
	<!-- ================================================ Form Bean Definitions -->
	<form-beans>
		<form-bean name="rateCalculatorForm" type="com.ff.rate.calculation.form.RateCalculatorForm"/>
		<form-bean name="rateQuotationForm" type="com.ff.rate.configuration.ratequotation.form.RateQuotationForm"/>
		<form-bean name="rateContractForm" type="com.ff.rate.configuration.ratecontract.form.RateContractForm"/>
		<form-bean name="rateBenchMarkForm" type="com.ff.rate.configuration.ratebenchmark.form.RateBenchMarkForm" />
		<form-bean name="regionRateBenchMarkDiscountForm" type="com.ff.rate.configuration.ratebenchmarkdiscount.form.RegionRateBenchMarkDiscountForm" />
		<form-bean name="cashRateConfigurationForm" type="com.ff.rate.configuration.rateConfiguration.form.CashRateConfigurationForm" />
		<form-bean name="baRateConfigurationForm" type="com.ff.rate.configuration.rateConfiguration.form.BARateConfigurationForm" />
		<form-bean name="ebRateConfigForm" type="com.ff.rate.configuration.ebrate.form.EBRateConfigForm" />
		<form-bean name="baMaterialRateConfigForm" type="com.ff.rate.configuration.rateConfiguration.form.BAMaterialRateConfigForm" />
	</form-beans>
	<!-- =========================================== Action Mapping Definitions -->
	
	<action-mappings>
	
	<action path="/viewCustomerSearchInfo" type="org.apache.struts.actions.ForwardAction" parameter="rateCustomerSearch" />
	
		<action path="/calculateRate"
			type="com.ff.rate.calculation.action.RateCalculaterAction" name="rateCalculatorForm"
			scope="request" parameter="submitName">
			<forward name="viewInputPage" path="/rate/RateCalculator.jsp" />
			<forward name="result" path="/rate/RateCalculatorResult.jsp" />
			<forward name="octroiResult" path="/rate/RateCalculatorOctroiResult.jsp" />
		</action>
		
		<action path="/rateQuotation" type="com.ff.rate.configuration.ratequotation.action.RateQuotationAction"
			name="rateQuotationForm" scope="request" parameter="submitName">
			<forward name="success"  path="rateQuotation" />
			<forward name="approve"  path="rateApproveQuotation"/>
			<forward name="successEcommerce" path="rateQuotationEcommerce"/>
			<forward name="normal"  path="rateQuotationNormal" />
			<forward name="ecommerce"  path="rateQuotationEcommerce" />
			<forward name="listView"  path="rateQuotationListView" />
			<forward name="viewNormalQuotation"  path="/rate/jsp/rateconfiguration/rateQuotation.jsp" />
			<forward name="viewEcommerceQuotation"  path="/rate/jsp/rateconfiguration/rateQuotationEcommerce.jsp" />
			<forward name="viewRateQuotationUpload"  path="rateQuotationUpload" />
			<forward name="rateQuotationStatusView" path="/jobService.do?submitName=jobServiceView" />
			<forward name="loginPage" path="/welcome.do" />
		</action>
		
		<action path="/rateBenchMarkDiscount" type="com.ff.rate.configuration.ratebenchmarkdiscount.action.RegionRateBenchMarkDiscountAction"
			name="regionRateBenchMarkDiscountForm" scope="request" parameter="submitName">	
			<forward name="success" path="viewBenchMarkDiscount"/>
			
			
		</action>
		
		<action path="/rateBenchMark" type="com.ff.rate.configuration.ratebenchmark.action.RateBenchMarkAction"
			name="rateBenchMarkForm" scope="request" parameter="submitName">	
			<forward name="success" path="rateBenchMark" />
		</action>
		
		<!-- Rate Contract START -->
		<action path="/rateContract" type="com.ff.rate.configuration.ratecontract.action.RateContractAction"
			name="rateContractForm" scope="request" parameter="submitName">
			<forward name="viewRateContractNormal" path="viewRateContractNormal"/>
			<forward name="viewRateContractECommerce" path="viewRateContractECommerce"/>
			<forward name="searchCustomerInfo" path="searchCustomerInfo"/>
			<forward name="listView"  path="rateContractListView" />
			<forward name="viewNormalContract"  path="/rate/jsp/rateconfiguration/rateContractNormal.jsp" />
			<forward name="viewEcommerceContract"  path="/rate/jsp/rateconfiguration/rateContractECommerce.jsp" />
		</action>
		<!-- Rate Contract END -->
		
		<!-- Rate Configuration START -->
		<action path="/cashRateConfiguration" type="com.ff.rate.configuration.rateConfiguration.action.CashRateConfigurationAction"
			name="cashRateConfigurationForm" scope="request" parameter="submitName">
			<forward name="viewCashRateConfiguration" path="viewCashRateConfiguration"/>
			<forward name="viewRenewCashRateConfiguration" path="/rate/jsp/rateconfiguration/cashRateConfiguration.jsp"/>
		</action>
		<action path="/baRateConfiguration"
			type="com.ff.rate.configuration.rateConfiguration.action.BARateConfigurationAction"
			name="baRateConfigurationForm" scope="request" parameter="submitName">
			<forward name="success" path="viewBARateConfiguration" />
			<forward name="renewBAConfiguration" path="viewRenewBAConfiguration" />
		</action>
		<action path="/baMaterialRateConfig"
			type="com.ff.rate.configuration.rateConfiguration.action.BAMaterialRateConfigAction"
			name="baMaterialRateConfigForm" scope="request" parameter="submitName">
			<forward name="viewBAMaterialRateConfig" path="viewBAMaterialRateConfig" />
			<forward name="renewBAMaterialRateConfig" path="/rate/jsp/rateconfiguration/baMaterialRateConfig.jsp" />
		</action>
		
		<!-- Rate Configuration END -->
		
		<!-- EB Rate Config START -->
		<action path="/emotionalBondRate"
			type="com.ff.rate.configuration.ebrate.action.EmotionalBondRateAction"
			name="ebRateConfigForm" scope="request" parameter="submitName">
			<forward name="viewEmotionalBondRateConfig" path="viewEmotionalBondRateConfig" />
		</action>
		<!-- EB Rate Config End -->
	
		
	</action-mappings>
	

	<message-resources key="rateResource" parameter="RateCalculationMessageResources_en_US" />

	<plug-in className="org.apache.struts.tiles.TilesPlugin">
		<set-property property="definitions-config" value="rate-tiles-defs.xml" />
		<set-property property="moduleAware" value="true" />
	</plug-in>

</struts-config>
