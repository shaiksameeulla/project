<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema version="2.0" xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://www.swacorp.com/cargo/event/data/v1"
	xmlns="http://www.swacorp.com/cargo/event/data/v1"
	xmlns:commonDataTypes="http://www.swacorp.com/cargo/common/services/data/v1">

	<xsd:import
		schemaLocation="../common/CommonDataTypes.xsd"
		namespace="http://www.swacorp.com/cargo/common/services/data/v1" />
		
	<xsd:element name="Event">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="header" type="EventHeader" />
				<xsd:element name="payload" type="EventPayload" maxOccurs="unbounded" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	
	<xsd:complexType name="EventPayload">
		<xsd:sequence>
				<xsd:element name="key" type="xsd:string" />
				<xsd:element name="value" type="commonDataTypes:Base" />
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="EventHeader">
		<xsd:sequence>
			<xsd:element name="eventDomain" type="xsd:string" minOccurs="1" />
			<xsd:element name="eventType" type="xsd:string" minOccurs="1" />
			<xsd:element name="eventSubTypes" type="xsd:string" nillable="true" minOccurs="0" maxOccurs="unbounded" />				
			<xsd:element name="eventVersion" type="xsd:string" minOccurs="0" />
			<xsd:element name="eventID" type="xsd:string" minOccurs="1" />
			<xsd:element name="eventCreationSystem" type="xsd:string" minOccurs="0" />
			<xsd:element name="eventCreationSystemInstanceId" type="xsd:long" minOccurs="0" />
			<xsd:element name="eventCreationHost" type="xsd:string" minOccurs="0" />
			<xsd:element name="eventCreatingProcessId" type="xsd:long" minOccurs="0" />
			<xsd:element name="eventCreationTime" type="xsd:dateTime" minOccurs="1" />
			<xsd:element name="eventCorrelationId" type="xsd:long" minOccurs="0" />
			<xsd:element name="entitySequenceNumber" type="xsd:string" minOccurs="0" />
			<xsd:element name="entityUpdateTimeStamp" type="xsd:dateTime" minOccurs="0" />
			<xsd:element name="entityKeys" type="EntityKey" minOccurs="0" maxOccurs="unbounded" />
			<xsd:element name="entityAttributes" type="StringAttribute" minOccurs="0" maxOccurs="unbounded" />
			<xsd:element name="awbStatusChangedTo" type="xsd:string" minOccurs="1" />
			<xsd:element name="sectionNumberUpdated" type="xsd:string" minOccurs="1" maxOccurs="unbounded" />
			<xsd:element name="reqSectionsModifiedAfterMAN" type="xsd:boolean" default="false"/>
			
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="EntityKey">
		<xsd:sequence>
				<xsd:element name="key" type="xsd:string" />
				<xsd:element name="value" type="xsd:anyType" />
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="StringAttribute">
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attribute name="name" type="xsd:string" />
				<xsd:attribute name="type" type="xsd:string" />
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>	

	<xsd:simpleType name="EventDomain">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="CARGOSYSTEM_CAPACITY_MGMT" />
			<xsd:enumeration value="CARGOSYSTEM_FACILITY_MGMT" />
			<xsd:enumeration value="CARGOSYSTEM_HANDLING_TIME" />
			<xsd:enumeration value="CARGOSYTEM_RESTRICTIONS" />
			<xsd:enumeration value="CARGOSYSTEM_ADMIN" />
			<xsd:enumeration value="CARGOSYSTEM_AWB_SHIPMENT" />
			<xsd:enumeration value="CARGOSYSTEM_CUSTOMER_MGMT" />
			<xsd:enumeration value="CARGOSYSTEM_AUDIT" />	
			<xsd:enumeration value="CARGOSYSTEM_CUSTOMS" />
			<xsd:enumeration value="CARGOSYSTEM_HAWB_MGMT" />
			<xsd:enumeration value="CARGOSYSTEM_INTERLINE_SCHEDULES" />
			<xsd:enumeration value="CARGOSYSTEM_RFS_MGMT" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="EventType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="CAPACITY_EVENT" />		
			<xsd:enumeration value="CAPACITY_PARAM_EVENT" />
			<xsd:enumeration value="AWB_SHIPMENT_EVENT" />			
			<xsd:enumeration value="MANAGE_FACILITY_EVENT" />
			<xsd:enumeration value="HANDLING_TIME_EVENT" />
			<xsd:enumeration value="MANAGE_RESTRICTIONS_EVENT" />
			<xsd:enumeration value="REFERENCE_DATA_EVENT" />	
			<xsd:enumeration value="ADMIN_EVENT" />
			<xsd:enumeration value="AWB_NUMBER_EVENT" />
			<xsd:enumeration value="CUSTOMER_MGMT_EVENT" />
			<xsd:enumeration value="CARGO_CUSTOMER_ACCOUNT" />
			<xsd:enumeration value="CARGOSYSTEM_AUDIT_EVENT" />
			<xsd:enumeration value="SVC_LVL_BUCKT_MPG_EVENT" />
			<xsd:enumeration value="COMMODITY_DESC_MAPNG_EVENT" />
			<xsd:enumeration value="CUSTOMS_EVENT" />
			<xsd:enumeration value="CUSTOMS_CARGOXML_EVENT" />
			<xsd:enumeration value="HAWB_EVENT" />
			<xsd:enumeration value="INTERLINE_SCHEDULES_EVENT" />
            <xsd:enumeration value="SCHED_EVENT" />
		</xsd:restriction>
	</xsd:simpleType>

