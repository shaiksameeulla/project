	<xsd:simpleType name="EventProducer">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="CARGO.CAPACITY_PARAM_MANAGEMENT_SERVICE" />
			<xsd:enumeration value="CARGO.CAPACITY_MANAGEMENT_SERVICE" />
