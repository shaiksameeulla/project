			<xsd:enumeration value="MANAGE_RESTRICTIONS_UPDATED" />
			<xsd:enumeration value="MANAGE_LIMITATION_UPDATED" />
			<xsd:enumeration value="MANAGE_BOOKING_WINDOW_SETUP" />
			<xsd:enumeration value="AWB_SHIPMENT_ADDED" />
