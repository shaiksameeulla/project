			<xsd:enumeration value="CARGO.ADMIN_EVENT_SERVICE" />	
			<xsd:enumeration value="CARGO.REFERENCE_DATA_EVENT_SERVICE" />	
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="EventConsumer">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="CARGO.EVENT_PUBLISHER_MODULE" />
